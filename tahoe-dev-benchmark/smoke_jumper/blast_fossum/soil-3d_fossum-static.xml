<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="ghsutley" geometry_file="./soil-3d2.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="Lin. Elast. Soil Test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="10" num_steps="100" output_inc="1" time_step="1e-2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="static">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="3" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="5"/>
            <kinematic_BC dof="1" node_ID="5"/>
	    <kinematic_BC dof="3" node_ID="5"/>
        </field>
    </nodes>
    <element_list>
        <small_strain_enh_loc choose_element="1" choose_normal="2"
            cohesion_softening_coefficient="1e3"
            dilation_softening_coefficient="0" field_name="displacement"
            friction_softening_coefficient="0" model_type="1"
            peak_cohesion="0" peak_dilation_angle_rad=".087"
            peak_friction_angle_rad=".5236" residual_cohesion="0" residual_friction_angle_rad="0">
            <hexahedron/>
            <natural_bc schedule="1" side_set_ID="6">
                <DoubleList>
                    <Double value="0"/>
                    <Double value="-50"/>
                    <Double value="0"/>
                </DoubleList>
            </natural_bc>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_enh_loc_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <Fossum_small_strain
                        cap_growth_parameter__D1__1_by_stress="3e-1"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless=".078"
                        cap_ratio__R__dimensionless="10" density="1.0"
                        fluidity_parameter__eta__dimensionless="0"
                        initial_cap_position__kappa_0__stress="-10"
                        plastic_potential_parameter_L_analagous_to_B="0"
                        plastic_potential_parameter_Phi_analagous_to_Theta="1.5e-3"
                        plastic_potential_parameter_Q_analagous_to_R="9"
                        shear_surface_growth_factor__c_alpha__stress="1600"
                        shear_surface_offset__N__stress="4"
                        shear_surface_parameter__A__stress="5.35"
                        shear_surface_parameter__B__1_by_stress="0"
                        shear_surface_parameter__C__stress="0"
                        shear_surface_parameter__theta__radians="1.8e-3" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1">
                        <bulk_and_shear bulk_modulus="7000" shear_modulus="750"/>
                    </Fossum_small_strain>
                </small_strain_material_3D>
            </small_strain_enh_loc_element_block>
        </small_strain_enh_loc>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+03" max_iterations="10" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>

