<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="ghsutley" geometry_file="./g3d.1x1.geom"
    output_format="automatic" restart_file="triaxial1.rs1000of2000"
    restart_output_inc="1000"
    title="SJ 3D biaxial compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="10" num_steps="1000" output_inc="1" time_step="1.0e-3">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="1.0" y="1.0"/>
                <OrderedPair x="2.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
        <OrderedPair x="1.0" y="2.85714285714284e-02"/>        
        <OrderedPair x="2.0" y="1.5"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="3" node_ID="6"/>
            <kinematic_BC dof="2" node_ID="2" schedule="2" type="u" value="-.1"/>
            <force_BC dof="1" node_ID="4" schedule="1" value="-15"/>
            <force_BC dof="3" node_ID="7" schedule="1" value="-15"/>
        </field>
        <history_node_ID_list>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain_enh_loc choose_element="1" choose_normal="2"
            cohesion_softening_coefficient="1e3"
            dilation_softening_coefficient="0" field_name="displacement"
            friction_softening_coefficient="0" model_type="1"
            peak_cohesion="0" peak_dilation_angle_rad=".087"
            peak_friction_angle_rad=".5236" residual_cohesion="0" residual_friction_angle_rad="0">
            <hexahedron/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_enh_loc_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <Fossum_small_strain
                        cap_growth_parameter__D1__1_by_stress="3e-1"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless=".078"
                        cap_ratio__R__dimensionless="10" density="1.0"
                        fluidity_parameter__eta__dimensionless="0"
                        initial_cap_position__kappa_0__stress="-190"
                        plastic_potential_parameter_L_analagous_to_B="0"
                        plastic_potential_parameter_Phi_analagous_to_Theta="2.8e-4"
                        plastic_potential_parameter_Q_analagous_to_R="9"
                        shear_surface_growth_factor__c_alpha__stress="275"
                        shear_surface_offset__N__stress="0.535"
                        shear_surface_parameter__A__stress="0.54"
                        shear_surface_parameter__B__1_by_stress="0"
                        shear_surface_parameter__C__stress="0"
                        shear_surface_parameter__theta__radians="3e-4" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1">
                        <bulk_and_shear bulk_modulus="7000" shear_modulus="140"/>
                    </Fossum_small_strain>
                </small_strain_material_3D>
            </small_strain_enh_loc_element_block>
        </small_strain_enh_loc>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-6"
        divergence_tolerance="1.0e+03" max_iterations="10" rel_tolerance="5.0e-6">
        <full_matrix/>
    </nonlinear_solver>
</tahoe>