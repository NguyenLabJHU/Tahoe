<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="ghsutley" geometry_file="./soil-3d.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="Lin. Elast. Soil Dynamic Test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="10" num_steps="400" output_inc="1" time_step="0.00001">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="0.001" y="1.0"/>
                <OrderedPair x="0.002" y="0.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="3" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="5"/>
            <kinematic_BC dof="1" node_ID="5"/>
            <kinematic_BC dof="3" node_ID="5"/>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant density="0.0025">
                        <E_and_nu Poisson_ratio="0.49" Young_modulus="417"/>
                    </small_strain_StVenant>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+03" max_iterations="50" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
