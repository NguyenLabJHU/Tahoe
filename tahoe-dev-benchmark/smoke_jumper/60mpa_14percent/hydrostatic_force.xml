<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="ghsutley" geometry_file="./g3d.1x1.geom"
    output_format="automatic" title="SJ 3D hydrostatic compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="10" num_steps="40" output_inc="1" time_step="2.5e-2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="3" node_ID="6"/>
            <force_BC dof="2" node_ID="2" schedule="1" value="-15.45"/>
            <force_BC dof="1" node_ID="4" schedule="1" value="-15.45"/>
            <force_BC dof="3" node_ID="7" schedule="1" value="-15.45"/>
        </field>
        <history_node_ID_list>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_GeoModel
                        cap_growth_parameter__D1__1_by_stress="3e-1"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless=".074"
                        cap_ratio__R__dimensionless="10" density="1"
                        fluidity_parameter__eta__dimensionless="1e-5"
                        initial_cap_position__kappa_0__stress="-178.2"
                        shear_surface_growth_factor__c_alpha__stress="1e5"
                        shear_surface_offset__N__stress=".1"
                        shear_surface_parameter__A__stress=".670"
                        shear_surface_parameter__B__1_by_stress="0"
                        shear_surface_parameter__C__stress="0"
                        shear_surface_parameter__theta__radians="3e-4" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1">
                        <bulk_and_shear bulk_modulus="7000" shear_modulus="19000"/>
                    </small_strain_GeoModel>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+03" max_iterations="50" rel_tolerance="1.0e-7">
        <full_matrix/>
    </nonlinear_solver>
</tahoe>
