    <time max_step_cuts="20" num_steps="20" output_inc="1" time_step="0.05">
        <schedule_function>
            <cosine_plus a=".5" b="-.5" c="10" d="0"
                description="f(t) = H(t,t1-t0)*(a + b cos(c t) + d sin(e t) + f t cos(g t) + p t sin(q t))"
                e="0" f="0" g="0" p="0" q="0" t0="0.0" t1="0.0"/>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="0.01" y="1"/>
                <OrderedPair x="0.02" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
