            <kinematic_BC dof="3" node_ID="3" type="fixed"/>
	    <kinematic_BC dof="3" node_ID="4" type="fixed"/>
            <force_BC dof="1" node_ID="5" schedule="2" value="-2.34"/>
	    <force_BC dof="1" node_ID="6" schedule="2" value="2.34"/>
	    <force_BC dof="1" node_ID="7" schedule="2" value="-2.34"/>
	    <force_BC dof="1" node_ID="8" schedule="2" value="2.34"/>
	    <force_BC dof="2" node_ID="9" schedule="2" value="-1.17"/>
	    <force_BC dof="2" node_ID="10" schedule="2" value="1.17"/>
	    <force_BC dof="2" node_ID="11" schedule="2" value="-1.17"/>
	    <force_BC dof="2" node_ID="12" schedule="2" value="1.17"/>
        </field>
        <field field_name="micro">
            <dof_labels>
                <String value="phi_11"/>
                <String value="phi_21"/>
                <String value="phi_31"/>
                <String value="phi_12"/>
                <String value="phi_22"/>
                <String value="phi_32"/>
                <String value="phi_13"/>
                <String value="phi_23"/>
                <String value="phi_33"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" type="fixed"/>
	    <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="17" type="fixed"/>
            <kinematic_BC dof="3" node_ID="17" type="fixed"/>
            <kinematic_BC dof="4" node_ID="17" type="fixed"/>
            <kinematic_BC dof="5" node_ID="3" type="fixed"/>
	    <kinematic_BC dof="5" node_ID="4" type="fixed"/>
            <kinematic_BC dof="6" node_ID="17" type="fixed"/>
	    <kinematic_BC dof="7" node_ID="17" type="fixed"/>
	    <kinematic_BC dof="8" node_ID="17" type="fixed"/>
            <kinematic_BC dof="9" node_ID="17" type="fixed"/>
        </field>
        <history_node_ID_list>
            <String value="3"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
