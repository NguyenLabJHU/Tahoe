<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="hspark" compute_IC="false"
    geometry_file="geometry/wire8.160ax160ax160a.geom"
    output_format="ExodusII" title="Bulk EAM CB" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1" output_inc="1" time_step="0.001"/>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <kinematic_BC dof="3" node_ID="2"/>
        </field>
    </nodes>
    <element_list>
        <total_lagrangian field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <FCC_EAM density="1.0">
                        <FCC_EAM_Cauchy-Born>
                            <FCC_natural/>
                            <Paradyn_EAM_glue parameter_file="auu3"/>
                        </FCC_EAM_Cauchy-Born>
                    </FCC_EAM>
                </large_strain_material_3D>
            </large_strain_element_block>
        </total_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-08"
        check_LHS_perturbation="1.0E-8" check_code="no_check"
        divergence_tolerance="1.0e+1" max_iterations="500" rel_tolerance="1.0e-10">
        <profile_matrix/>
        <LOBPCG_solver block_size="2" max_eigenmodes="2"
            max_iter="50000" output_matricies="false" tolerance="1.0E-5"
            use_relative_tolerance="false" verbosity="IterationDetails"/>
    </nonlinear_solver>
</tahoe>
