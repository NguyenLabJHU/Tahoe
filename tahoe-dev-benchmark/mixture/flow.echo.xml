<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
             author='paklein'
      geometry_file='./simulations/1D-transport/bar.10.exo'
      output_format='ExodusII'
 restart_output_inc='100'
              title='simple flow'
           xmlns:x0='http://www.w3.org/2001/XMLSchema'>
	<time
	 max_step_cuts='0'
	     num_steps='100'
	    output_inc='1'
	     time_step='1.0'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.0'
				 y='0.0'/>
				<OrderedPair
				 x='1.0'
				 y='1.0'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.0'
				 y='1.0'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.0'
				 y='1.0'/>
				<OrderedPair
				 x='1.0'
				 y='1.1'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='10.0'
				 y='0.0'/>
				<OrderedPair
				 x='10.01'
				 y='1.0'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		 field_name='displacement'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
				<String
				 value='D_Z'/>
			</dof_labels>
			<kinematic_BC
			     dof='1'
			 node_ID='4'/>
			<kinematic_BC
			     dof='2'
			 node_ID='4'/>
			<kinematic_BC
			     dof='3'
			 node_ID='4'/>
		</field>
		<field
		     field_name='solid'
		     integrator='trapezoid'
		 solution_group='2'>
			<dof_labels>
				<String
				 value='c_s'/>
			</dof_labels>
			<initial_condition
			 all_nodes='true'
			       dof='1'
			      type='u'
			     value='1.0'/>
			<kinematic_BC
			      dof='1'
			  node_ID='4'
			 schedule='2'
			     type='u'
			    value='1.0'/>
		</field>
		<field
		     field_name='fluid'
		     integrator='trapezoid'
		 solution_group='3'>
			<dof_labels>
				<String
				 value='c_f'/>
			</dof_labels>
			<initial_condition
			 all_nodes='true'
			       dof='1'
			      type='u'
			     value='1.0'/>
			<kinematic_BC
			      dof='1'
			  node_ID='1'
			 schedule='2'
			     type='u'
			    value='1.01'/>
			<kinematic_BC
			      dof='1'
			  node_ID='2'
			 schedule='2'
			     type='u'
			    value='0.99'/>
		</field>
		<field
		     field_name='solute_1'
		     integrator='trapezoid'
		 solution_group='4'>
			<dof_labels>
				<String
				 value='c_1'/>
			</dof_labels>
			<initial_condition
			 all_nodes='true'
			       dof='1'
			      type='u'
			     value='0.05'/>
			<force_BC
			      dof='1'
			  node_ID='1'
			 schedule='2'
			    value='0.0'/>
		</field>
	</nodes>
	<element_list>
		<updated_lagrangian_mixture
		 field_name='displacement'>
			<hexahedron/>
			<solid_element_nodal_output
			   displacements='1'
			 material_output='1'
			          stress='1'>
				<wave_direction>
					<Vector_3
					 v_1='1.0'
					 v_2='0.0'
					 v_3='0.0'/>
				</wave_direction>
			</solid_element_nodal_output>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_3D>
					<large_strain_solid_mixture
					 density='1.0'>
						<solid_mixture_species
						 concentration_field='solid'>
							<large_strain_StVenant
							 density='1.0'>
								<E_and_nu
								 Poisson_ratio='0.25'
								 Young_modulus='100.0'/>
							</large_strain_StVenant>
						</solid_mixture_species>
						<solid_mixture_species
						 concentration_field='fluid'>
							<large_strain_StVenant
							 density='1.0e-01'>
								<E_and_nu
								 Poisson_ratio='0.25'
								 Young_modulus='100.0'/>
							</large_strain_StVenant>
						</solid_mixture_species>
						<solid_mixture_species
						 concentration_field='solute_1'>
							<large_strain_StVenant
							 density='1.0e-01'>
								<E_and_nu
								 Poisson_ratio='0.25'
								 Young_modulus='1.0e-10'/>
							</large_strain_StVenant>
						</solid_mixture_species>
					</large_strain_solid_mixture>
				</large_strain_material_3D>
			</large_strain_element_block>
		</updated_lagrangian_mixture>
		<mixture_species
		          concentration='current_concentration'
		             field_name='fluid'
		            output_mass='true'
		    solid_element_group='1'
		 stress_gradient_option='element_projection'>
			<hexahedron/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					  conductivity='1.0e-02'
					       density='1.0'
					 specific_heat='1.0'/>
				</diffusion_material>
			</diffusion_element_block>
		</mixture_species>
		<mixture_species
		     background_species='fluid'
		          concentration='current_concentration'
		             field_name='solute_1'
		            output_mass='true'
		    solid_element_group='1'
		 stress_gradient_option='element_projection'>
			<hexahedron/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					  conductivity='1.0e-02'
					       density='1.0'
					 specific_heat='1.0'/>
				</diffusion_material>
			</diffusion_element_block>
		</mixture_species>
	</element_list>
	<nonlinear_solver
	          abs_tolerance='1.0e-10'
	 check_LHS_perturbation='1.0e-08'
	             check_code='no_check'
	   divergence_tolerance='1.0e+10'
	         max_iterations='5'
	            print_eqnos='false'
	          rel_tolerance='1.0e-12'>
		<full_matrix/>
	</nonlinear_solver>
	<nonlinear_solver
	        abs_tolerance='1.0e-10'
	 divergence_tolerance='1.0e+10'
	       max_iterations='100'
	          print_eqnos='false'
	        rel_tolerance='1.0e-10'>
		<profile_matrix/>
	</nonlinear_solver>
	<nonlinear_solver_LS
	          abs_tolerance='1.0e-10'
	   divergence_tolerance='1.0e+10'
	 line_search_iterations='3'
	  line_search_tolerance='2.5E-1'
	         max_iterations='100'
	               max_step='2.5E0'
	            print_eqnos='false'
	          rel_tolerance='1.0e-10'>
		<profile_matrix/>
	</nonlinear_solver_LS>
	<nonlinear_solver
	        abs_tolerance='1.0e-10'
	 divergence_tolerance='1.0e+10'
	       max_iterations='100'
	        rel_tolerance='1.0e-10'>
		<profile_matrix/>
	</nonlinear_solver>
	<solver_phases
	 max_loops='100'>
		<solver_phase
		      iterations='-1'
		 pass_iterations='0'
		          solver='1'/>
		<solver_phase
		      iterations='-1'
		 pass_iterations='0'
		          solver='2'/>
		<solver_phase
		      iterations='-1'
		 pass_iterations='0'
		          solver='3'/>
		<solver_phase
		      iterations='-1'
		 pass_iterations='0'
		          solver='4'/>
	</solver_phases>
</tahoe>
