<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='simple mixture'
             author='paklein'
    geometry_format='automatic'
      geometry_file='./geometry/cube.1.geom'
      output_format='automatic'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	     num_steps='2'
	    output_inc='1'
	 max_step_cuts='0'
	     time_step='5.000000e-01'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='1.000000e-01'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='1.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='1.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='1.100000e+00'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
				<String
				 value='D_Z'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='3'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='2'
			      dof='3'
			     type='u'
			 schedule='1'
			    value='1.000000e-01'/>
			<kinematic_BC
			  node_ID='3'
			      dof='2'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='4'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
		</field>
		<field
		              field_name='solid'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='c_s'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='5'
			      dof='1'
			     type='u'
			 schedule='2'
			    value='5.000000e-01'/>
		</field>
		<field
		              field_name='fluid'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='c_f'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='1'
			     type='u'
			 schedule='2'
			    value='5.000000e-01'/>
			<kinematic_BC
			  node_ID='6'
			      dof='1'
			     type='u'
			 schedule='3'
			    value='5.000000e-01'/>
		</field>
	</nodes>
	<element_list>
		<updated_lagrangian_mixture
		     field_name='displacement'
		      mass_type='automatic'
		 eigenvalue_inc='0'>
			<hexahedron
			 num_ip='8'/>
			<solid_element_nodal_output
			   displacements='1'
			          stress='1'
			     wave_speeds='1'
			 material_output='1'>
				<wave_direction>
					<Vector_3
					 v_1='1.000000e+00'
					 v_2='0.000000e+00'
					 v_3='0.000000e+00'/>
				</wave_direction>
			</solid_element_nodal_output>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_3D>
					<large_strain_solid_mixture
					       density='1.000000e+00'
					 constraint_2D='none'
					           CTE='0.000000e+00'>
						<solid_mixture_species
						 concentration_field='solid'>
							<large_strain_StVenant
							       density='1.000000e+00'
							 constraint_2D='none'
							           CTE='0.000000e+00'>
								<E_and_nu
								 Young_modulus='1.000000e+02'
								 Poisson_ratio='2.500000e-01'/>
							</large_strain_StVenant>
						</solid_mixture_species>
						<solid_mixture_species
						 concentration_field='fluid'>
							<large_strain_StVenant
							       density='1.000000e+00'
							 constraint_2D='none'
							           CTE='0.000000e+00'>
								<E_and_nu
								 Young_modulus='1.000000e+02'
								 Poisson_ratio='2.500000e-01'/>
							</large_strain_StVenant>
						</solid_mixture_species>
					</large_strain_solid_mixture>
				</large_strain_material_3D>
			</large_strain_element_block>
		</updated_lagrangian_mixture>
	</element_list>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+01'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
</tahoe>
