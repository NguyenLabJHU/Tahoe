<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="./geometry/cube.1.geom"
    title="simple mixture" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="0.1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
                <OrderedPair x="1.0" y="1.1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="3" node_ID="2" schedule="1" type="u" value="0.1"/>
            <kinematic_BC dof="2" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
        <field field_name="solid">
            <dof_labels>
                <String value="c_s"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="5" schedule="2" type="u" value="0.5"/>
        </field>
        <field field_name="fluid">
            <dof_labels>
                <String value="c_f"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" schedule="2" type="u" value="0.5"/>
            <kinematic_BC dof="1" node_ID="6" schedule="3" type="u" value="0.5"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian_mixture field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1" wave_speeds="1">
                <wave_direction>
                    <Vector_3 v_1="1.0" v_2="0.0" v_3="0.0"/>
                </wave_direction>
            </solid_element_nodal_output>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <large_strain_solid_mixture density="1.0">
                        <solid_mixture_species concentration_field="solid">
                            <large_strain_StVenant density="1.0">
                                <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                            </large_strain_StVenant>
                        </solid_mixture_species>
                        <solid_mixture_species concentration_field="fluid">
                            <large_strain_StVenant density="1.0">
                                <E_and_nu Poisson_ratio="0.25" Young_modulus="100.0"/>
                            </large_strain_StVenant>
                        </solid_mixture_species>
                    </large_strain_solid_mixture>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian_mixture>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
