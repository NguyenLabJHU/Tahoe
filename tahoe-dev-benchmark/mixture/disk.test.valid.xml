<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='contracting fiber'
             author='paklein'
    geometry_format='ExodusII'
      geometry_file='./simulations/disk/geometry/disk.exo'
      output_format='ExodusII'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	     num_steps='10'
	    output_inc='1'
	 max_step_cuts='0'
	     time_step='1.000000e-03'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='1.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='1.000000e+00'/>
				<OrderedPair
				 x='1.000000e+00'
				 y='0.000000e+00'/>
			</piecewise_linear>
		</schedule_function>
		<schedule_function>
			<power_law
			 a='1.000000e+00'
			 b='1.000000e+00'
			 c='-1.000000e+00'
			 n='2.000000e+00'/>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
				<String
				 value='D_Z'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='3'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='2'
			      dof='3'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<mapped_nodes
			 schedule='1'>
				<Matrix_3x3
				 copy_symmetric='false'
				          A_1_1='-1.000000e+00'
				          A_2_1='0.000000e+00'
				          A_3_1='0.000000e+00'
				          A_1_2='0.000000e+00'
				          A_2_2='-1.000000e+00'
				          A_3_2='0.000000e+00'
				          A_1_3='0.000000e+00'
				          A_2_3='0.000000e+00'
				          A_3_3='0.000000e+00'/>
				<mapped_node_ID_list>
					<String
					 value='4'/>
				</mapped_node_ID_list>
			</mapped_nodes>
		</field>
		<field
		              field_name='solid'
		          solution_group='2'
		              integrator='trapezoid'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='c_s'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='4'
			      dof='1'
			     type='u'
			 schedule='0'
			    value='5.000000e-01'/>
		</field>
		<field
		              field_name='fluid'
		          solution_group='3'
		              integrator='trapezoid'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='c_f'/>
			</dof_labels>
			<initial_condition
			 all_nodes='true'
			       dof='1'
			      type='u'
			     value='5.000000e-01'/>
			<kinematic_BC
			  node_ID='3'
			      dof='1'
			     type='u'
			 schedule='3'
			    value='5.000000e-01'/>
		</field>
	</nodes>
	<element_list>
		<updated_lagrangian_mixture
		     field_name='displacement'
		      mass_type='automatic'
		 eigenvalue_inc='0'>
			<hexahedron
			 num_ip='27'/>
			<solid_element_nodal_output
			   displacements='1'
			 material_output='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_3D>
					<large_strain_solid_mixture
					       density='1.000000e+00'
					 constraint_2D='none'
					           CTE='0.000000e+00'>
						<solid_mixture_species
						 concentration_field='solid'>
							<large_strain_StVenant
							       density='1.000000e+00'
							 constraint_2D='none'
							           CTE='0.000000e+00'>
								<E_and_nu
								 Young_modulus='1.000000e+02'
								 Poisson_ratio='2.500000e-01'/>
							</large_strain_StVenant>
						</solid_mixture_species>
						<solid_mixture_species
						 concentration_field='fluid'>
							<large_strain_StVenant
							       density='1.000000e+00'
							 constraint_2D='none'
							           CTE='0.000000e+00'>
								<E_and_nu
								 Young_modulus='1.000000e+02'
								 Poisson_ratio='2.500000e-01'/>
							</large_strain_StVenant>
						</solid_mixture_species>
					</large_strain_solid_mixture>
				</large_strain_material_3D>
			</large_strain_element_block>
		</updated_lagrangian_mixture>
		<current_mixture_species
		             field_name='fluid'
		    solid_element_group='1'
		           species_type='fluid'
		 stress_gradient_option='global_projection'
		            output_mass='true'>
			<hexahedron
			 num_ip='27'/>
			<diffusion_element_nodal_output
			    displacement='1'
			 material_output='1'/>
			<diffusion_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<diffusion_material>
					<linear_diffusion_material
					       density='1.000000e+00'
					 specific_heat='1.000000e+00'
					  conductivity='1.000000e-01'/>
				</diffusion_material>
			</diffusion_element_block>
		</current_mixture_species>
	</element_list>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+10'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='10'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-12'
	      divergence_tolerance='1.000000e+10'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
	<nonlinear_solver_LS
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='100'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-10'
	             rel_tolerance='1.000000e-06'
	      divergence_tolerance='1.000000e+10'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'
	    line_search_iterations='5'
	     line_search_tolerance='1.000000e-01'
	                  max_step='2.500000e+00'>
		<profile_matrix/>
	</nonlinear_solver_LS>
	<solver_phases
	 max_loops='100'>
		<solver_phase
		          solver='1'
		      iterations='-1'
		 pass_iterations='0'/>
		<solver_phase
		          solver='2'
		      iterations='-1'
		 pass_iterations='0'/>
		<solver_phase
		          solver='3'
		      iterations='-1'
		 pass_iterations='0'/>
	</solver_phases>
</tahoe>
