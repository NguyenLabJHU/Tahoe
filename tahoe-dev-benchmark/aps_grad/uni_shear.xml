<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="raregue" geometry_file="./geometry/quad4qdr.geom"
    title="2D anti-plane uniform shear" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_A"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="3" schedule="1" type="u" value="1.0"/>
        </field>
        <field field_name="plast">
            <dof_labels>
                <String value="gammap_x_A"/>
                <String value="gammap_y_A"/>
            </dof_labels>
        </field>
    </nodes>
    <element_list>
        <antiplane_shear_grad_plast GeometryCodeSurf_displ="0"
            GeometryCode_displ="1" H_param="0.1" NumIPSurf_displ="3"
            NumIP_displ="9" field_name="displ" gamma0_dot_1="1.0e-4"
            gamma0_dot_2="0" gamma0_dot_3="0" kappa0_1=".2"
            kappa0_2="200" kappa0_3="200" length_scale="0" m1_x="1"
            m1_y="0" m2_x="0" m2_y="1" m3_x="0" m3_y="0" n_en_displ="8"
            n_en_plast="4" num_sidesets="0" plast_mod_type="1"
            plastic_grad_field_name="plast" rate_param="10" shear_modulus="1">
            <aps_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </aps_element_block>
        </antiplane_shear_grad_plast>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-14"
        divergence_tolerance="1.0e+01" max_iterations="20" rel_tolerance="1.0e-14">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
