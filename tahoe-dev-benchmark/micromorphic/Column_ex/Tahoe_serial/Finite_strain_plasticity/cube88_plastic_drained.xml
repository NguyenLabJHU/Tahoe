<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="../../cube88.geom" geometry_format="TahoeII"
    output_format="ExodusII"
    title="3x3x3 27-node element mesh with punch" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="5" num_steps="500" output_inc="10" time_step="0.002">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="3" node_ID="2" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
            <kinematic_BC dof="1" node_ID="6" type="fixed"/>
            <kinematic_BC dof="2" node_ID="7" type="fixed"/>
            <kinematic_BC dof="3" node_ID="8" schedule="1" type="u" value="-10"/>
        </field>
        <field field_name="press">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="5" type="fixed"/>
        </field>
        <history_node_ID_list>
            <String value="8"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix Beta="-1"
            GeometryCodeSurf_displ="1" GeometryCode_displ="3" H_c="1e-5"
            K="1e-5" Kf="2.2e-2" NumIPSurf_displ="9" NumIP_displ="27"
            Phi=".61" Psi=".61" R="10" alpha_k="1000" c0="5e-5"
            constitutive_mod_type="2" field_name="displ" g="9.8" g1="0"
            g2="0" g3="0"
            initial_condition_1geostatic_2displ_vel_press="1"
            kappa0="-5e-8" lambda="6.4e-3" local_tol_absolute="1e-8"
            local_tol_relative="1e-8" local_yield_tr_tol="1e-8"
            max_local_iterations="20" mu="6.4e-3" n_en_displ="27"
            n_en_press="8" phi_f0=".42" phi_s0=".58"
            pore_pressure_field_name="press" rho_fR0="1000"
            rho_sR0="2700" type_of_analysis_0static_1consolidation_2dynamic="0">
            <total_lagrangian_solid_fluid_mix_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_element_block>
            <gravity_body_force gravity_schedule="1"/>
        </total_lagrangian_solid_fluid_mix>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+4" max_iterations="20" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
