<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./geometry/column4.geom" geometry_format="TahoeII"
    output_format="ExodusII"
    title="1x1x4 27-node element mesh with punch" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="5" num_steps="50" output_inc="1" time_step="0.02">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="3" node_ID="2" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
            <kinematic_BC dof="1" node_ID="6" type="fixed"/>
            <kinematic_BC dof="2" node_ID="7" type="fixed"/>
            <kinematic_BC dof="3" node_ID="8" schedule="1" type="u" value="-10"/>
        </field>
        <field field_name="micro">
            <dof_labels>
                <String value="phi_11"/>
                <String value="phi_21"/>
                <String value="phi_31"/>
                <String value="phi_12"/>
                <String value="phi_22"/>
                <String value="phi_32"/>
                <String value="phi_13"/>
                <String value="phi_23"/>
                <String value="phi_33"/>
            </dof_labels>     
            <kinematic_BC dof="2" node_ID="5" type="fixed"/>
            <kinematic_BC dof="3" node_ID="5" type="fixed"/>
            <kinematic_BC dof="4" node_ID="5" type="fixed"/>          
            <kinematic_BC dof="6" node_ID="5" type="fixed"/>
            <kinematic_BC dof="7" node_ID="5" type="fixed"/>
            <kinematic_BC dof="8" node_ID="5" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
            <kinematic_BC dof="5" node_ID="1" type="fixed"/>
            <kinematic_BC dof="9" node_ID="2" type="fixed"/>
            <kinematic_BC dof="5" node_ID="7" type="fixed"/>
            <kinematic_BC dof="1" node_ID="6" type="fixed"/>                                    
        </field>
        <history_node_ID_list>
            <String value="8"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <micromorphic_FS_3D Eta="12.8e-3" GeometryCodeSurf_displ="1"
            GeometryCode_displ="3" Kappa="8.96e-3" Nu="7.4112e-3"
            NumIPSurf_displ="9" NumIP_displ="27" Sigma_const="1.92e-3"
            Tau="2.133e-3" Tau1="0.0" Tau10="0.0" Tau11="0.0" Tau2="0.0" Tau3="0.0"
            Tau4="0.0" Tau5="0.0" Tau6="0.0" Tau7="0.0" Tau8="0.0" Tau9="0.0"
            constitutive_mod_type="1" field_name="displ" g="9.81" g1="0"
            g2="0" g3="0" lambda="4.2667e-3" micro_field_name="micro"
            mu="4.48e-3" n_en_displ="27" n_en_micro="8"
            ndof_per_nd_micro="9" rho_0="2700" max_local_iterations="20"
            local_tol_absolute="1e-8" local_tol_relative="1e-8" Hc="0.0"
            local_yield_tr_tol="1e-8" Fphi="0.0" Dpsi="0.0" 
            c0="0.0">
            <micromorphic_FS_3D_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </micromorphic_FS_3D_element_block>
        </micromorphic_FS_3D>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+4" max_iterations="30" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
