<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="raregue" geometry_file="../../geometry/cube.1.geom"
    title="3D uniaxial compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <graph_decomposition/>
    <time max_step_cuts="5" num_steps="30" output_inc="1" time_step="1.0e2">
        <schedule_function>
            <linear_exponential a="0" b="0" c="-2" d="1000"/>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="3" node_ID="2" schedule="1" type="u" value="1"/>
            <kinematic_BC dof="2" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <solid_element_element_output material_output="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <SIERRA_EMMI SIERRA_parameter_file="./emmi.1.params" density="16.6e3">
                        <E_and_nu Poisson_ratio=".34" Young_modulus="168.e9"/>
                    </SIERRA_EMMI>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="25" rel_tolerance="1.0e-08">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
