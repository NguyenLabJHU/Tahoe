##
## native Sierra input parameters
##
  begin definition for function ZERO
    type is piecewise linear
    ordinate is delta
    abscissa is time
    begin values
        0.00    0.000
        3000.0  0.000
    end values
  end definition for function ZERO

  begin definition for function TEMPERATURE
    type is piecewise linear
    ordinate is temperature
    abscissa is time
    begin values
        0.00   298.0 
        3000.0 298.0 
    end values
  end definition for function TEMPERATURE

  begin definition for function THERMAL_STRAIN
    type is piecewise linear
    ordinate is strain
    abscissa is temperature
    begin values
      200.0  0.0
     3000.0  0.0
    end values
  end definition for function THERMAL_STRAIN

  begin property specification for material Ta
    density       = 16.6e3  $kg/m^3
    thermal strain function = THERMAL_STRAIN

##    begin parameters for model elastic
##      youngs modulus =  168.0E9 $Pa
##      poissons ratio  = 0.34
##    end parameters for model elastic

    begin parameters for model EMMI
      YOUNGS MODULUS = 168.0E9	$(Pa)
      POISSONS RATIO = 0.34	$(.)
	RGAS	= 8.314		$(J/(K mol)) gas constant
	BVECT	= 2.86e-10	$(m) Burgers vector
	D0	= 1.2e-5	$((m^2)/s) pre-exponential diffusivity coefficient
	QD	= 4.13e5	$(J/mol) activation energy
	RHO	= 16.6e3	$(kg/m^3) initial mass density
	CV	= 142.0		$(J/(K kg)) specific heat at constant volume
	BDRAG   = 0.0  		$(s) drag coefficient
	ADRAG   = 0.0		$(.) drag intercept
	DMTHETA	= 0.42		$(.) shear modulus temperature coefficient
	DMPHI	= 1.0		$(.) shear modulus damage coefficient
	DNTHETA	= 0.42		$(.) bulk modulus temperature coefficient
	DNPHI	= 1.0		$(.) bulk modulus damage coefficient
	THETA0	= 298.0		$(K) reference temperature
	THETAM	= 3271.0	$(K) melt temperature
#	BETA0	= 6.5e-6	$(1/K) coefficient of thermal expansion at reference temperature
	BETA0	= 0.0e0		$(1/K) coefficient of thermal expansion at reference temperature
#	BTHETA	= 0.42		$(.) thermal expansion temperature coefficient
	BTHETA	= 0.0		$(.) thermal expansion temperature coefficient
	DMR	= 4.2		$(.) damage rate sensitivity parameter
        DNUC1   = 0.0           $(.) damage nucleation parameter
        DNUC2   = 0.0           $(.) damage nucleation parameter
        DNUC3   = 0.0           $(.) damage nucleation parameter
        DNUC4   = 0.0           $(.) damage nucleation parameter
	DM1	= 9.5		$(.) coefficient of yield temperature dependence
	DM2	= 7.1		$(.) coefficient of yield temperature dependence
	DM3ND 	= 0.083		$(.) coefficient of yield temperature dependence
	DM4ND 	= 2.94		$(.) coefficient of yield temperature dependence
	DM5ND 	= 0.046		$(.) coefficient of yield temperature dependence
	Q1ND	= 0.113		$(.) dimensionless activation energy for f
	Q2ND	= 0.0183	$(.) dimensionless activation energy for rd
	Q3ND	= 0.3   	$(.) dimensionless activation energy for Rd
	Q4ND	= 9.85   	$(.) dimensionless activation energy for Rs
	CALPHA	= 2.0		$(.) coefficient for backstress alpha
	CKAPPA	= 2.0		$(.) coefficient for internal stress kappa
	C1	= 1.09e-5	$(.) parameter of flow rule exponent n
	C2ND	= 7.53e-14	$(.) parameter for transition rate f
	C3	= 0.0		$(.) parameter for alpha dynamic recovery rd
	C4	= 0.0		$(.) parameter for alpha hardening h
	C5	= 117.0		$(.) parameter for kappa dynamic recovery Rd
	C6	= 4.5e-3	$(.) parameter for kappa hardening H
	C7ND	= 5.37e-6	$(.) parameter for kappa static recovery Rs
	C8ND	= 7.73e-4	$(.) parameter for yield
	C9ND	= 0.736 	$(.) parameter for temperature dependence of flow rule exponent n
	C10	= 1.0		$(.) parameter in static recovery (set=1)
	A1	= 0.0		$(.) plastic anisotropy parameter
	A2	= 0.0		$(.) plastic anisotropy parameter
	A3	= 0.0		$(.) plastic anisotropy parameter
	A4	= 0.0		$(.) plastic anisotropy parameter
	A_XX	= 0.0		$(.) initial structure tensor component A_11
	A_YY	= 0.0		$(.) initial structure tensor component A_22
	A_ZZ	= 0.0		$(.) initial structure tensor component A_33
	A_XY	= 0.0		$(.) initial structure tensor component A_12
	A_YZ	= 0.0		$(.) initial structure tensor component A_23
	A_XZ	= 0.0		$(.) initial structure tensor component A_13
	ALPHA_XX	= 0.0	$(Pa) initial backstress component ALPHA_11
	ALPHA_YY	= 0.0	$(Pa) initial backstress component ALPHA_22
	ALPHA_ZZ	= 0.0	$(Pa) initial backstress component ALPHA_33
	ALPHA_XY	= 0.0	$(Pa) initial backstress component ALPHA_12
	ALPHA_YZ	= 0.0	$(Pa) initial backstress component ALPHA_23
	ALPHA_XZ	= 0.0	$(Pa) initial backstress component ALPHA_13
	DKAPPA	= 0.0		$(Pa) initial isotropic internal stress
#	PHI0	= 1.0e-5	$(.) initial isotropic porosity
	PHI0	= 0.0e0		$(.) initial isotropic porosity
	PHI_CR	= 0.9e0		$(.) critical porosity
	DLAMBDAG	= 0.0	$(.) slip system geometry parameter
	FACTOR		= 0.0	$(.) (0<FACTOR<1) fraction of plastic work converted to heat, adiabatic
	R_SWITCH= 1.0		$(.) rate sensitivity switch: only with D_IMPL_OPT = 4.0
#                               $    0.0: turn off rate sensitivity
#                               $    1.0: turn on rate sensitivity
	DMG_OPT		= 1.0	$(.) damage model option parameter:
#                               $    1.0: pressure independent Cocks/Ashby 1980
#                               $    2.0: pressure dependent Cocks/Ashby 1980
#                               $    3.0: pressure dependent Cocks 1989
	THETA_OPT	= 1.0	$(.) temperature option parameter:
#                               $    0.0: drive temperature external to model
#                               $    1.0: adiabatic temperature evolution from plastic work (set FACTOR)
        D_IMPL_OPT      = 3.0   $(.) implementation option flag:
#                               $    1.0: combined viscous drag, thermally activated dislocation motion
#                               $         implicit in mag of plastic deformation rate and damage
#                               $    2.0: separate viscous drag, thermally activated dislocation motion
#                               $         implicit in mag of plastic deformation rate and damage
#                               $    3.0: separate viscous drag, thermally activated dislocation motion
#                               $         implicit in mag of plastic deformation rate, explicit damage
#                               $    4.0: separate viscous drag, thermally activated dislocation motion
#                               $         implicit in mag of plastic deformation rate with approx., explicit damage
#                               $         only implemented for pressure independent Cocks/Ashby 1980 damage
	DNITER	= 100   	$(.) maximum number of iterations for local Newton iteration
	ATOL  	= 1.0e-10	$(.) absolute error tolerance for local Newton iteration
	RTOL  	= 1.0e-8	$(.) relative error tolerance for local Newton iteration
    end parameters for model EMMI

  end property specification for material Ta
