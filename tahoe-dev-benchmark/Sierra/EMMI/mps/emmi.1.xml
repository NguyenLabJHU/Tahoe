<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="cube.geom"
    title="3D uniaxial extension" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="100" output_inc="1000" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="10.0" y="10.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <mapped_nodes schedule="1">
                <Matrix_3x3 A_1_1="0.01"/>
                <mapped_node_ID_list>
                    <String value="1"/>
                </mapped_node_ID_list>
            </mapped_nodes>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian field_name="displacement">
            <hexahedron num_ip="1"/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <SIERRA_EMMI SIERRA_parameter_file="./emmi.1.params"
                        debug="true" density="16.6e3">
                        <E_and_nu Poisson_ratio=".34" Young_modulus="168.e9"/>
                    </SIERRA_EMMI>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="25" rel_tolerance="1.0e-08">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
