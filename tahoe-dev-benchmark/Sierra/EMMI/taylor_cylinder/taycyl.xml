<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="raregue" echo_input="true" geometry_file="taycyl.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="taylor_cylinder" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <graph_decomposition/>
    <time num_steps="900" output_inc="100" time_step="1e-8">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="central_difference">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <initial_condition all_nodes="true" dof="3" type="D_u" value="-175"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="3" node_ID="20"/>
        </field>
    </nodes>
    <element_list>
        <updated_lagrangian_Q1P0 field_name="displacement" mass_type="consistent_mass">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <solid_element_element_output kinetic_energy="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <SIERRA_EMMI SIERRA_parameter_file="emmi.1.params" density="16.6e3">
                        <E_and_nu Poisson_ratio=".34" Young_modulus="168e9"/>
                    </SIERRA_EMMI>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian_Q1P0>
    </element_list>
    <linear_solver>
        <SPOOLES_matrix/>
    </linear_solver>
</tahoe>
