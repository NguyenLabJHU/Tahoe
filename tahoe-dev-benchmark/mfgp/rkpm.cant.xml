<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="karma" geometry_file="./2by2beam.geom"
    output_format="Tahoe" title="rkpm_cant" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="5" output_inc="1" time_step="0.2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <force_BC dof="2" node_ID="2" schedule="1" value="-0.00005"/>
        </field>
    </nodes>
    <element_list>
        <small_strain_meshfree field_name="displacement">
            <quadrilateral num_ip="4"/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_MR_2D density="1.0">
                        <E_and_nu Poisson_ratio=".25" Young_modulus="100.0"/>
                        <MR_SS_nonlinear_hardening Gf_I="2" Gf_II="1"
                            Tol_1="1.0e-8" Tol_2="1.0e-8" alpha_c="10"
                            alpha_chi="100" alpha_phi="10"
                            alpha_psi="10" c_p="4.5" c_r="0" chi_p="3"
                            chi_r=".1" phi_p=".72" phi_r=".58" psi_p=".72"/>
                    </small_strain_StVenant_MR_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
            <meshfree_support_2D>
                <RKPM completeness="1">
                    <gaussian_window cutoff_factor="2.0"
                        sharpening_factor="0.4" support_scaling="1.0"/>
                </RKPM>
            </meshfree_support_2D>
            <meshfree_fracture_support>
                <interpolant_node_ID_list>
                    <String value="1"/>
                    <String value="2"/>
                </interpolant_node_ID_list>
            </meshfree_fracture_support>
        </small_strain_meshfree>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+01" max_iterations="30" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
