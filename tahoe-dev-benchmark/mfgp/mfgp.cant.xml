<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="Karma" geometry_file="./2by2beam.geom" title="mfgp_cant" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="5" output_inc="1" time_step="0.2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="1"/>
            <force_BC dof="2" node_ID="2" schedule="1" value="-0.00005"/>
        </field>
        <field field_name="plast">
            <dof_labels>
                <String value="lambda"/>
            </dof_labels>
        </field>
    </nodes>
    <element_list>
        <mfgp_element field_name="displ" plastic_mult_field_name="plast">
            <quadrilateral/>
            <mfgp_element_nodal_output displacements="1" lambdas="1"
                lap_lambda="1" stress="1"/>
            <mfgp_element_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <mfgp_material_2D>
                    <small_strain_StVenant_MR_grad_2D density="1">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100"/>
                        <GRAD_MR_SS_nonlinear_hardening Gf_I="2"
                            Gf_II="1" Tol_1="1.0e-8" alpha_c="10"
                            alpha_chi="100" alpha_phi="10"
                            alpha_psi="10" c_p="4.5" c_r="0.0" chi_p="3"
                            chi_r="0.1" lse_s="0.0" lse_v="0.0"
                            lsp_s="0.0" lsp_v="0.0" phi_p="0.72"
                            phi_r="0.58" psi_p="0.72"/>
                    </small_strain_StVenant_MR_grad_2D>
                </mfgp_material_2D>
            </mfgp_element_element_block>
            <D3_meshfree_support_2D>
                <RKPM completeness="1">
                    <gaussian_window cutoff_factor="2.0E0"
                        sharpening_factor="4.0E-1" support_scaling="1.0E0"/>
                </RKPM>
            </D3_meshfree_support_2D>
            <meshfree_fracture_support>
                <interpolant_node_ID_list>
                    <String value="1"/>
                    <String value="2"/>
                </interpolant_node_ID_list>
            </meshfree_fracture_support>
        </mfgp_element>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+1" max_iterations="30" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
