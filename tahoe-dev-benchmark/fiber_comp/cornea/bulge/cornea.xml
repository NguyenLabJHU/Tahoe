                        </matrix_material_params>
                        <eq_fibril_potential>
                            <fung_type alpha="0.121" beta="34.7" description="f(I) = alpha*(exp(beta*(I - 1.0)) + beta/I)"/>
                        </eq_fibril_potential>
                        <neq_fibril_potential>
