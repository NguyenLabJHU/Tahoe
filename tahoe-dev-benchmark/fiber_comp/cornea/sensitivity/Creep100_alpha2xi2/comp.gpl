!export BEST=`grep "Best data captured at function evaluation" qnewton.stdout | awk '{print $8}'` ; cp in.${BEST} in.best; cp creep100.${BEST}.uvst_Y creep100.best.uvst_Y ; cp creep100.${BEST}.shift creep100.best.shift; echo BEST ${BEST}

set xlabel "TIME"
set ylabel "DISPLACEMENT"
set logscale xy
plot "creep100.best.uvst_Y" t "best" w l, "creep100.best.shift" t "shifted" w l, "creep100.dat" t "data" w lp, "../creep100.base.uvst_Y" t "baseline" w l
pause -1
set term post eps color
set output "comp.eps"
repl
set term png color
set output "comp.png"
repl
