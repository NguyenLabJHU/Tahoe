strategy, \
	single_method, \
	#graphics

method, \
	optpp_q_newton, \
	convergence_tolerance = 1e-5 \
	output verbose \

variables, \
  continuous_design = 2 \
  cdv_descriptor     'ALPHA' 'BETA' \
  cdv_initial_point   0.1     40.0 \
  cdv_lower_bounds    0.01    30.0 \
  cdv_upper_bounds   10.0     50.0 \

interface, \
	system \
	analysis_driver = 'run.pl' \
	parameters_file = 'in' \
	results_file = 'out' \
	file_tag \
	file_save

responses, \
	num_objective_functions = 1 \
	numerical_gradients \
	method_source dakota \
	interval_type central \
	fd_gradient_step_size = 1.0e-6\
	no_hessians

