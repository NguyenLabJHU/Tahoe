strategy, \
  single_method \
  tabular_graphics_data \
#  graphics \

method, \
  nond_sampling \
  samples = 20 seed = 17 \
  response_levels = 10.0 \
  sample_type lhs

variables, \
  loguniform_uncertain = 10 \
  luuv_lower_bounds   0.01   1.0   1\
  luuv_upper_bounds   1.00 100.0 100\
  luuv_descriptor     'ALPHA0' 'BETA' 'MU'

interface,                                              \
        system async evaluation_concurrency = 1                              \
          analysis_driver =    'run.pl'   \
          parameters_file = 'in'                 \
          results_file =    'out'               \
          file_tag                                      \
          file_save                                     \

responses, \
  num_response_functions = 1 \
  no_gradients \
  no_hessians  
