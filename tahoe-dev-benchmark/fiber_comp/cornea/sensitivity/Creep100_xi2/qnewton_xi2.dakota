strategy, \
	single_method, \
	#graphics

method, \
	optpp_q_newton, \
	convergence_tolerance = 1e-4 \
	output verbose \

variables, \
  continuous_design = 1 \
  cdv_scales          1.0  \
  cdv_descriptor     'XI2' \
  cdv_initial_point  100.0 \
  cdv_lower_bounds    10.0   \
  cdv_upper_bounds  1000000.0 

interface, \
	system \
	analysis_driver = 'run.pl' \
	parameters_file = 'in' \
	results_file = 'out' \
	file_tag \
	file_save

responses, \
	num_objective_functions = 1 \
	numerical_gradients \
	  method_source dakota \
	  interval_type central \
	  fd_gradient_step_size = 1.0e-4 \
	no_hessians

