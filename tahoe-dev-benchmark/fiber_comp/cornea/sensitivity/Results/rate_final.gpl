set xlabel "Time(s)"
set ylabel "Engineering Strain (mm/mm)"
set key left box
plot \
"rate3_5.dat"  using (($1)):(($2)/2) t "Experiment 3.5 kPa/s"  w p,\
"rate3_5.uvst_Y" using 1:(($2)/2) t "Simulation 3.5 kPa/s" w l
pause -1
set term png color
set output "rate3_5.png"
repl
set term post eps color
set output "rate3_5.eps"
repl
set term x11
set output 
