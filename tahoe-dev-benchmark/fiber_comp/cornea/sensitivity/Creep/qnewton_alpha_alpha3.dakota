strategy, \
	single_method, \
	#graphics

method, \
	optpp_q_newton, \
	convergence_tolerance = 1e-4 \
	output verbose \

variables, \
  continuous_design = 2 \
  cdv_scales          1.0      1.0    \
  cdv_descriptor     'ALPHA' 'ALPHA3' \
  cdv_initial_point   0.381   0.381   \
  cdv_lower_bounds    0.001   0.001   \
  cdv_upper_bounds    2.0     2.0    

interface, \
	system \
	analysis_driver = 'run.pl' \
	parameters_file = 'in' \
	results_file = 'out' \
	file_tag \
	file_save

responses, \
	num_objective_functions = 1 \
  num_nonlinear_equality_constraints = 1\
    nonlinear_equality_targets 1.143\
    nonlinear_equality_scales 1.0\
	numerical_gradients \
	  method_source dakota \
	  interval_type central \
	  fd_gradient_step_size = 1.0e-4 \
	no_hessians

