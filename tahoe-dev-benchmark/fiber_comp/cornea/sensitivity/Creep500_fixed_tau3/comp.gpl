!export BEST=`grep "Best data captured at function evaluation" qnewton.stdout | awk '{print $8}'` ; cp in.${BEST} in.best; cp creep500.${BEST}.uvst_Y creep500.best.uvst_Y ; cp creep500.${BEST}.shift creep500.best.shift; echo BEST ${BEST}; cat in.best

set xlabel "TIME"
set ylabel "DISPLACEMENT"
set logscale xy
set yrange [0.04:0.16]
plot "creep500.best.uvst_Y" t "best" w l, "creep500.best.shift" t "shifted" w l, "creep500.dat" t "data" w lp, "creep100.dat" t "data-100" w lp
#plot "creep500.best.uvst_Y" t "best" w l, "creep500.best.shift" t "shifted" w l, "creep500.dat" t "data" w lp, "../creep500.base.uvst_Y" t "baseline" w l, "creep100.dat" t "data-100" w lp
##plot "creep500.best.uvst_Y" t "best" w l, "creep500.best.shift" t "shifted" w l
pause -1
set term post eps color
set output "comp.eps"
repl
set term png color
set output "comp.png"
