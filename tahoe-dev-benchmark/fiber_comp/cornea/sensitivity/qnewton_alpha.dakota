strategy, \
	single_method, \
	#graphics

method, \
	optpp_q_newton, \
	convergence_tolerance = 1e-4 \
	output verbose \

variables, \
  continuous_design = 1 \
  cdv_descriptor     'ALPHA' \
  cdv_initial_point   0.1 \
  cdv_lower_bounds    0.001 \
  cdv_upper_bounds  100.0 \

interface, \
	system \
	analysis_driver = 'run.pl' \
	parameters_file = 'in' \
	results_file = 'out' \
	file_tag \
	file_save

responses, \
	num_objective_functions = 1 \
	numerical_gradients \
	method_source dakota \
	interval_type central \
	fd_gradient_step_size = 1.0e-4 \
	no_hessians

