<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="tdnguye" geometry_file="../geometry/cube_8.exo"
    geometry_format="ExodusII" output_format="ExodusII"
    restart_output_inc="10000" title="fiber_comp_test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="10" num_steps="10000" output_inc="1" time_step=".01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0"/>
                <OrderedPair x="0.001" y="1"/>
                <OrderedPair x="1.0" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="6" type="fixed"/>
            <kinematic_BC dof="3" node_ID="3" schedule="1" type="u" value="1.0"/>
            <kinematic_BC dof="3" node_ID="4" type="fixed"/>
        </field>
        <history_node_ID_list>
            <String value="5"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <uplag_fiber_comp_planar field_name="displacement">
            <hexahedron num_ip="8"/>
            <solid_element_nodal_output coordinates="1"
                displacements="1" material_output="1" stress="1"/>
            <fiber_comp_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <fiber_comp_material>
                    <quasilinear_viscoelasticity_Nfibers density="1" use_for_all_fibers="false">
                        <eq_matrix_potential>
                            <mooney-rivlin c1=".2" c2="0" kappa="1000"/>
                        </eq_matrix_potential>
                        <neq_matrix_potential>
                            <mooney-rivlin c1="1.8" c2="0" kappa="0"/>
                        </neq_matrix_potential>
                        <matrix_relax_time tau_m="10"/>
                        <qlv_fibers_params>
                            <eq_fiber_pot>
                                <parabola b="0" k="2" l0="1"/>
                            </eq_fiber_pot>
                            <neq_fiber_pot>
                                <parabola b="0" k="38" l0="1"/>
                            </neq_fiber_pot>
                            <fiber_relax_time tau_f="10"/>
                        </qlv_fibers_params>
                        <qlv_fibers_params>
                            <eq_fiber_pot>
                                <parabola b="0" k="1" l0="1"/>
                            </eq_fiber_pot>
                            <neq_fiber_pot>
                                <parabola b="0" k="19" l0="1"/>
                            </neq_fiber_pot>
                            <fiber_relax_time tau_f="10"/>
                        </qlv_fibers_params>
                    </quasilinear_viscoelasticity_Nfibers>
                </fiber_comp_material>
            </fiber_comp_element_block>
            <fiber_orientations>
                <side_set coordinate_system="global" side_set_ID="1">
                    <DoubleList>
                        <Double value="1.0"/>
                        <Double value="0.0"/>
                        <Double value="0.0"/>
                    </DoubleList>
                    <DoubleList>
                        <Double value="0.0"/>
                        <Double value="1.0"/>
                        <Double value="0.0"/>
                    </DoubleList>
                </side_set>
            </fiber_orientations>
        </uplag_fiber_comp_planar>
    </element_list>
    <nonlinear_solver abs_tolerance="1e-8" divergence_tolerance="100"
        max_iterations="10" rel_tolerance="1e-8">
        <SPOOLES_matrix/>
    </nonlinear_solver>
</tahoe>
