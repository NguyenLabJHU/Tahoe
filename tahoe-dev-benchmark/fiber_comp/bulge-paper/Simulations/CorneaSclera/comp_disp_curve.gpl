set grid
set xrange [0:*]
set xlabel "(CUURENT) RADIAL COORDINATE (mm)"
set ylabel "OUT-OF-PLANE DISPLACEMENT (mm)"
plot "cornea_iso/u_nt.dat" using (($2)+($5)):7 t "NT-cornea_iso" w l, \
     "cornea_aniso/u_nt.dat" using (($2)+($5)):7 t "NT-cornea_aniso" w l, \
     "sclera_iso/u_nt.dat" using (($2)+($5)):7 t "NT-sclera_iso" w l, \
     "sclera_aniso/u_nt.dat" using (($2)+($5)):7 t "NT-sclera_aniso" w l
pause -1 "hit any key to continue"
set term png
set output "comp_nt.png"
repl
