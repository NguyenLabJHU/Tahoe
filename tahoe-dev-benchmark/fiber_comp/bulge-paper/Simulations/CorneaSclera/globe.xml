<?xml version="1.0" encoding="UTF-8"?>
<tahoe geometry_file="./globe.exo" geometry_format="ExodusII"
    output_format="ExodusII" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="2" num_steps="10" output_inc="1" time_step="1.">
        <schedule_function>
            <piecewise_linear>
<OrderedPair x="0" y="0."/>
<OrderedPair x="10" y="10.0"/>
<!--
<OrderedPair x="1" y="1.06"/>
<OrderedPair x="2" y="1.991"/>
<OrderedPair x="3" y="3.043"/>
<OrderedPair x="4" y="3.974"/>
<OrderedPair x="5" y="5.026"/>
<OrderedPair x="6" y="5.945"/>
<OrderedPair x="7" y="7.008"/>
<OrderedPair x="8" y="8.06"/>
<OrderedPair x="9" y="8.991"/>
<OrderedPair x="10" y="10.043"/>
-->
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="10" type="fixed"/>
            <kinematic_BC dof="2" node_ID="20" type="fixed"/>
            <kinematic_BC dof="3" node_ID="30" type="fixed"/>
<!--
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="2" type="fixed"/>
            <kinematic_BC dof="3" node_ID="2" type="fixed"/>
-->
            <pressure_bc schedule="1">
                <side_set_ID_list>
                    <String value="41"/>
                    <String value="42"/>
                </side_set_ID_list>
            </pressure_bc>
        </field>
        <history_node_ID_list>
            <String value="100"/>
            <String value="101"/>
            <String value="102"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <uplag_fiber_comp_planar field_name="displacement">
            <hexahedron num_ip="8"/>
            <solid_element_nodal_output coordinates="1"
                displacements="1" material_output="1"
                principal_stress="1" strain_energy_density="1" stress="1"/>
            <fiber_comp_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <fiber_comp_material>
                   <aniso_viscoelastic_cornea density="1.0" n_points="11">
                        <matrix_material_params>
                            <Neo-Hookean bulk_modulus="30.0" shear_modulus="10"/>
                        </matrix_material_params>
                        <eq_fibril_potential>
                            <fung_type alpha="0.00008" 
                                       beta="55.0" description="f(I) = alpha*(exp(beta*(I - 1.0)) + beta/I)"/>
                        </eq_fibril_potential>
                        <neq_fibril_potential>
                            <fung_type alpha="0.00008" 
                                       beta="55.0"/>
                        </neq_fibril_potential>
                        <neq_fibril_potential>
                            <fung_type alpha="0.00008" 
                                       beta="55.0"/>
                        </neq_fibril_potential>
                        <neq_fibril_potential>
                            <fung_type alpha="0.00008" 
                                       beta="55.0"/>
                        </neq_fibril_potential>
                       <viscosity>
                            <scaled_csch
                                description="f(tau) = eta0* tau/tau0 / Sinh(tau/tau0)"
                                eta0="1e+8" tau0="1e+5"/>
                        </viscosity>
                        <viscosity>
                            <scaled_csch
                                description="f(tau) = eta0* tau/tau0 / Sinh(tau/tau0)"
                                eta0="1e+8"
                                tau0="1e+5"/>
                        </viscosity>
                        <viscosity>
                            <scaled_csch
                                description="f(tau) = eta0* tau/tau0 / Sinh(tau/tau0)"
                                eta0="1e+8"
                                tau0="1e+5"/>
                        </viscosity>
                        <fibril_distribution>
<!--
                            <power_trig a="0.0" b="0.0" c="1.0" n="2" phi="0.0"/>
-->
                            <cornea_mod a1="0.859353" a2="0"
                                aa2="0.136947" b1="0.859353"
                                b2="1.28414" c1="0.530042" c2="0.536528"
                                n1="8" n2="8" r1="6" r2="11" r3="12.857" xi="45"/>
                        </fibril_distribution>
                    </aniso_viscoelastic_cornea>
                </fiber_comp_material>
            </fiber_comp_element_block>
            <fiber_orientations>
                <ellipsoid Cx="0.0E1" Cy="0.0E1" Cz="-24.193"
                    Rx="17.9829" Ry="17.7144" Rz="24.193" block_ID="1"
                    coordinate_system="cartesian" projection_normal="z">
                    <DoubleList name="NT_">
                        <Double value="1"/>
                        <Double value="0"/>
                    </DoubleList>
                    <DoubleList name="IS_">
                        <Double value="0"/>
                        <Double value="1"/>
                    </DoubleList>
                </ellipsoid>
            </fiber_orientations>
        </uplag_fiber_comp_planar>
        <updated_lagrangian field_name="displacement">
            <hexahedron num_ip="8"/>
            <solid_element_nodal_output coordinates="1"
                displacements="1" material_output="1"
                principal_stress="1" strain_energy_density="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <large_strain_StVenant density="1.0">
                        <E_and_nu Poisson_ratio="0.4" Young_modulus="5000"/>
                    </large_strain_StVenant>
<!--
                    <RG_split_general density="1">
                        <rg_eq_potential>
                            <veronda-westmann alpha="0.00032" beta="55"
                                gamma="0" kappa="10000"/>
                        </rg_eq_potential>
                    </RG_split_general>
-->
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-8" check_code="small_pivots"
        divergence_tolerance="1.0e+03" max_iterations="12"
        output_inc="0" rel_tolerance="1.0e-8">
        <full_matrix/>
    </nonlinear_solver>
</tahoe>
