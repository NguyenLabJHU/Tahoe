<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="rdorgan" compute_IC="true" echo_input="false"
    geometry_file="../geometry/bar.009el.geom" output_format="Tahoe"
    title="1D bar under tension" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="1" num_steps="100" output_inc="1" time_step="0.01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2" schedule="1" type="u" value="0.1"/>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <line/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_1D>
                    <small_strain_StVenant_J2_1D density="1.0">
                        <E_and_nu Poisson_ratio="0.3" Young_modulus="2.0e+04"/>
                        <linear_function a="-2.0e+03" b="2.0e+00"/>
                    </small_strain_StVenant_J2_1D>
                </small_strain_material_1D>
            </small_strain_element_block>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <small_strain_material_1D>
                    <small_strain_StVenant_J2_1D density="1.0">
                        <E_and_nu Poisson_ratio="0.3" Young_modulus="2.0e+04"/>
                        <linear_function a="-2.0e+03" b="1.8e+00"/>
                    </small_strain_StVenant_J2_1D>
                </small_strain_material_1D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1e-10" divergence_tolerance="1e+02"
        max_iterations="100" rel_tolerance="1e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
