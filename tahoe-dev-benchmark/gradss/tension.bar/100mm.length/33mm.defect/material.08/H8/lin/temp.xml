<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="rdorgan" compute_IC="true" echo_input="false"
    geometry_file="../geometry/bar.021elx010elx001el.geom"
    output_format="EnSight" title="3D bar under tension" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="5" num_steps="100" output_inc="1" time_step="0.01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="3" schedule="1" type="u" value="1.5"/>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="1" node_ID="5"/>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant_J2 density="1.0">
                        <E_and_nu Poisson_ratio="0.49" Young_modulus="1.192e+04"/>
                        <linear_function a="-4e+02" b="100.0"/>
                    </small_strain_StVenant_J2>
                </small_strain_material_3D>
            </small_strain_element_block>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant_J2 density="1.0">
                        <E_and_nu Poisson_ratio="0.49" Young_modulus="1.192e+04"/>
                        <linear_function a="-4e+02" b="90.0"/>
                    </small_strain_StVenant_J2>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1e-10" divergence_tolerance="1e+02"
        max_iterations="50" rel_tolerance="1e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
