<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" geometry_file="cse.rotation.geom"
    title="simple MR rigid CZ test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="1000" output_inc="1" time_step=".001">
        <schedule_function>
            <cosine a="0.7071067812" b="-1" c="1.570796327" d="0.7853981634"/>
        </schedule_function>
        <schedule_function>
            <cosine a="-0.7071067812" b="1" c="-1.570796327" d="0.7853981634"/>
        </schedule_function>
        <schedule_function>
            <cosine a="0" b="-1" c="1.570796327" d="1.570796327"/>
        </schedule_function>
        <schedule_function>
            <cosine a="-1" b="1" c="1.570796327" d="0"/>
        </schedule_function>
        <schedule_function>
            <cosine a="1" b="-1" c="1.5708" d="0"/>
        </schedule_function>
        <schedule_function>
            <cosine a="0" b="1" c="-1.5708" d="1.5708"/>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="6"/>
            <kinematic_BC dof="2" node_ID="6"/>
            <kinematic_BC dof="1" node_ID="4" schedule="1" type="u" value="1.414213562"/>
            <kinematic_BC dof="2" node_ID="4" schedule="2" type="u" value="1.414213562"/>
            <kinematic_BC dof="1" node_ID="5" schedule="3" type="u" value="1e0"/>
            <kinematic_BC dof="2" node_ID="5" schedule="4" type="u" value="1e0"/>
            <kinematic_BC dof="1" node_ID="3" schedule="5" type="u" value="1e0"/>
            <kinematic_BC dof="2" node_ID="3" schedule="6" type="u" value="1e0"/>
        </field>
        <history_node_ID_list>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_2D density="1">
                        <E_and_nu Poisson_ratio=".3" Young_modulus="1e5"/>
                    </small_strain_StVenant_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <nodal_rigid_anisotropic_CSE_MR field_name="displacement">
            <line/>
            <surface_element_nodal_output displacement_jump="1"
                displacements="1" material_output="1" traction="1"/>
            <anisotropic_CSE_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <cohesive_relation_2D>
                    <nodal-rigid-plastic_MR_RP2D Gf_I="2" Gf_II="2"
                        Tol_1="1e-8" Tol_2="1e-8" alpha_c="10"
                        alpha_chi="100" alpha_phi="10" alpha_psi="10"
                        c_p="4.5" c_r="0.0" chi_p="3.0" chi_r="0.0"
                        phi_p="0.7" phi_r="0.0" psi_p="0.5"/>
                </cohesive_relation_2D>
            </anisotropic_CSE_element_block>
        </nodal_rigid_anisotropic_CSE_MR>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+10" max_iterations="100" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
