<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="skyu" geometry_file="cse.comp.geom"
    title="simple 2D MR cz test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="3" num_steps="1000" output_inc="1" time_step=".001">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1" value="0"/>
            <kinematic_BC dof="2" node_ID="2" schedule="1" type="u" value="-0.006"/>
            <kinematic_BC dof="1" node_ID="11" value="0"/>
        </field>
        <history_node_ID_list>
            <String value="1"/>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_DP_2D
                        constraint_2D="plane_strain" density="1.0">
                        <E_and_nu Poisson_ratio="0.3" Young_modulus="1.5e7"/>
                        <DP_SS_linear_hardening H_prime="0"
                            alpha_bar="0.6e4" dilation="0.35" friction="0.5"/>
                    </small_strain_StVenant_DP_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <anisotropic_CSE field_name="displacement">
            <line/>
            <surface_element_nodal_output displacements="1"
                material_output="1" traction="1"/>
            <anisotropic_CSE_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <cohesive_relation_2D>
                    <elastoplastic_MR_2D E_n="1.5e8" E_t=".5769e8"
                        Gf_I="200" Gf_II="100" Tol_1="1e-8" Tol_2="1e-8"
                        alpha_c="10" alpha_chi="10" alpha_phi="10"
                        alpha_psi="10" c_p="7.85e3" c_r="1e3"
                        chi_p="25e3" chi_r="25e2" phi_p="0.436"
                        phi_r="0.1" psi_p="0.2618"/>
                </cohesive_relation_2D>
            </anisotropic_CSE_element_block>
        </anisotropic_CSE>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-8"
        divergence_tolerance="1.0e+2" max_iterations="50" output_inc="0"
        quick_solve_count="3" quick_solve_iter="6" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
