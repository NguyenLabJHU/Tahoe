<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="skyu" geometry_file="../Geometry/shale_2D_1.exo"
    title="2D MR cz test for shale_1 tension case" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <graph_decomposition/>
    <time max_step_cuts="5" num_steps="1000" output_inc="1" time_step=".001">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.1" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.1" y="1"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="2" schedule="2" type="u" value="-2e-2"/>
            <kinematic_BC dof="1" node_ID="3"/>
        </field>
        <history_node_ID_list>
            <String value="1"/>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <triangle/>
            <natural_bc coordinate_system="global" schedule="3" side_set_ID="2">
                <DoubleList>
                    <Double value="-1"/>
                    <Double value="0.0"/>
                </DoubleList>
            </natural_bc>
            <natural_bc coordinate_system="global" schedule="3" side_set_ID="4">
                <DoubleList>
                    <Double value="1"/>
                    <Double value="0.0"/>
                </DoubleList>
            </natural_bc>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <solid_element_element_output material_output="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_DP_2D
                        constraint_2D="plane_strain" density="1">
                        <E_and_nu Poisson_ratio=".24" Young_modulus="3e3"/>
                        <DP_SS_linear_hardening H_prime="-1"
                            alpha_bar="6.1" dilation=".3" friction=".436"/>
                    </small_strain_StVenant_DP_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <small_strain field_name="displacement">
            <triangle/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_2D
                        constraint_2D="plane_strain" density="1">
                        <E_and_nu Poisson_ratio="0.09" Young_modulus="1e5"/>
                    </small_strain_StVenant_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <anisotropic_CSE field_name="displacement">
            <line/>
            <surface_element_nodal_output displacements="1"
                material_output="1" traction="1"/>
            <anisotropic_CSE_element_block>
                <block_ID_list>
                    <String value="3"/>
                </block_ID_list>
                <cohesive_relation_2D>
                    <elastoplastic_MR_2D E_n="1e7" E_t="1e7" Gf_I="1e2"
                        Gf_II="1e2" Tol_1="1e-6" Tol_2="1e-6"
                        alpha_c="100" alpha_chi="100" alpha_phi="10"
                        alpha_psi="10" c_p="4.0" c_r="0.5" chi_p="0.5"
                        chi_r="0.1" phi_p="0.4" phi_r="0.1" psi_p="0.3"/>
                </cohesive_relation_2D>
            </anisotropic_CSE_element_block>
        </anisotropic_CSE>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-8"
        divergence_tolerance="1.0e+4" max_iterations="50" output_inc="0"
        quick_solve_count="3" quick_solve_iter="6" rel_tolerance="1.0e-8">
        <SPOOLES_matrix/>
    </nonlinear_solver>
</tahoe>
