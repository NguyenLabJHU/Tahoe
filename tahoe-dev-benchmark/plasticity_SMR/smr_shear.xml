<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="karma" geometry_file="cube1elem.geom" title="shear loading" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="60" output_inc="1" time_step="1.">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"></OrderedPair>
                <OrderedPair x="5.0" y="1.0"></OrderedPair>
                <OrderedPair x="60.0" y="1.0"></OrderedPair>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"></OrderedPair>
                <OrderedPair x="5.0" y="0.25"></OrderedPair>
                <OrderedPair x="60.0" y="1.0"></OrderedPair>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"></String>
                <String value="D_Y"></String>
                <String value="D_Z"></String>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" type="fixed"></kinematic_BC>
            <kinematic_BC dof="2" node_ID="2" type="fixed"></kinematic_BC>
            <kinematic_BC dof="3" node_ID="3" type="fixed"></kinematic_BC>
            <kinematic_BC dof="1" node_ID="7" schedule="2" type="u" value="-0.4"></kinematic_BC>
            <force_BC dof="1" node_ID="4" schedule="1" value="-5.0"></force_BC>
            <force_BC dof="2" node_ID="5" schedule="1" value="-5.0"></force_BC>
            <force_BC dof="3" node_ID="6" schedule="1" value="-5.0"></force_BC>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron num_ip="8"></hexahedron>
            <solid_element_nodal_output displacements="1" stress="1"></solid_element_nodal_output>
            <solid_element_element_output stress="1"></solid_element_element_output>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"></String>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant_SMR density="1.0">
                        <E_and_nu Poisson_ratio=".3" Young_modulus="100."></E_and_nu>
                        <SMR_SS_nonlinear_hardening Gf_II="1." Tol_1="1.0e-6" Tol_2="1.0e-8" alpha_phi="0.25" alpha_psi="0." phi_p="30." phi_r="15." psi_p="30." c="5."></SMR_SS_nonlinear_hardening>
                    </small_strain_StVenant_SMR>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-4" divergence_tolerance="1.0e+01" max_iterations="60" rel_tolerance="1.0e-5">
        <profile_matrix></profile_matrix>
    </nonlinear_solver>
</tahoe>
