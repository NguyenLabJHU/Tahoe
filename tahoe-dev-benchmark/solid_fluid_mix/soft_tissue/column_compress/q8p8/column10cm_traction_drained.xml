<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./column_mesh.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="ten 8-node elements 3D uniaxial strain compression" 
    xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="0" num_steps="10" output_inc="1" time_step="0.01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x=".1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="3" node_ID="3" type="fixed"/>
            <kinematic_BC dof="2" node_ID="4" type="fixed"/>
            <kinematic_BC dof="1" node_ID="5" type="fixed"/>
            <force_BC dof="3" node_ID="6" schedule="1"  value="-0.025"/>
        </field>
        <field field_name="press">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="7" type="fixed"/>
        </field>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix_q8p8
            field_name="displ" pore_pressure_field_name="press" 
            n_en_displ="8" n_en_press="8" 
            GeometryCodeSurf_displ="1" GeometryCode_displ="3" 
            NumIPSurf_displ="4" NumIP_displ="8" 
            K="1.83e-7" Kf="2.2e9" 
            phi_f0="0.99" phi_s0="0.01"
            rho_fR0="1000" rho_sR0="1000" 
            constitutive_mod_type="1" 
            mu="1923" lambda="2885" 
            Beta="-1" c0="0" H_c="0" Phi=".35" Psi=".1745" 
            R="10" alpha_k="100" kappa0="-2000" 
            Alpha="0"
            local_tol_absolute="1e-8" local_tol_relative="1e-8" 
            local_yield_tr_tol="1e-1" max_local_iterations="20" 
            g="9.81" g1="0" g2="0" g3="0"
            initial_condition_1geostatic_2displ_vel_press="1"
            type_of_analysis_0static_1consolidation_2dynamic="0">
            <total_lagrangian_solid_fluid_mix_q8p8_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_q8p8_element_block>
            <gravity_body_force gravity_schedule="1"/>
        </total_lagrangian_solid_fluid_mix_q8p8>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+2" max_iterations="10" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
