<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="../geometry/cube.hex27.natural.ver2.geom"
    geometry_format="TahoeII" output_format="Tahoe"
    title="one 27-node element 3D compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="7" num_steps="1" output_inc="1" time_step="0.005">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <initial_condition dof="3" node_ID="4" type="u" value="-0.1"/>
            <initial_condition dof="3" node_ID="4" type="D_u" value="-0.5"/>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="3"/>
        </field>
        <field field_name="press" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <initial_condition dof="1" node_ID="5" type="u" value="-9.81"/>
        </field>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix GeometryCodeSurf_displ="1"
            GeometryCode_displ="3" K="0.1" Kf="22000000000"
            NumIPSurf_displ="9" NumIP_displ="27" alpha="1" beta="0.25"
            constitutive_mod_type="1" field_name="displ" g="9.81" g1="0"
            g2="0" g3="-9.81" gamma="0.5" lambda="29000000" mu="7000000"
            n_en_displ="27" n_en_press="8" phi_f0="0.42" phi_s0="0.58"
            pore_pressure_field_name="press" rho_fR0="1000" rho_sR0="2700">
            <total_lagrangian_solid_fluid_mix_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_element_block>
        </total_lagrangian_solid_fluid_mix>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+2" max_iterations="10" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
