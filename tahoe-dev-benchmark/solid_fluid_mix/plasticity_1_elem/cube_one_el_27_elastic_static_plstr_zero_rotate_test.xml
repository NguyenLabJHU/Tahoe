        </schedule_function>
        <schedule_function>
            <cosine_plus a="-2" b="2" c="1.5708" d="2" e="1.5708" f="0" g="1.5708" p="0" q="1.5708"/>
        </schedule_function>
        <schedule_function>
            <cosine_plus a="-2" b="2" c="1.5708" d="-1" e="1.5708" f="0" g="1.5708" p="0" q="1.5708"/>
