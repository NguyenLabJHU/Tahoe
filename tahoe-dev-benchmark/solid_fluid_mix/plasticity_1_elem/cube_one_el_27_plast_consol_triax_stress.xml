<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./cube.hex27.geom" geometry_format="TahoeII"
    output_format="Tahoe"
    title="one 27-node element triaxial compression consol" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="0" num_steps="80" output_inc="1" time_step=".025">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
                <OrderedPair x="2.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
                <OrderedPair x="2.0" y="2.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ" integrator="trapezoid">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1" type="fixed"/>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="3" type="fixed"/>
            <force_BC dof="1" node_ID="8" schedule="1" value="-2000"/>
            <force_BC dof="1" node_ID="9" schedule="1" value="-8000"/>
            <force_BC dof="1" node_ID="10" schedule="1" value="-32000"/>
            <force_BC dof="2" node_ID="11" schedule="1" value="-2000"/>
            <force_BC dof="2" node_ID="12" schedule="1" value="-8000"/>
            <force_BC dof="2" node_ID="13" schedule="1" value="-32000"/>
            <force_BC dof="3" node_ID="14" schedule="2" value="-2000"/>
            <force_BC dof="3" node_ID="15" schedule="2" value="-8000"/>
            <force_BC dof="3" node_ID="16" schedule="2" value="-32000"/>
        </field>
        <field field_name="press" integrator="trapezoid">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="14" type="fixed"/>
        </field>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix Beta="-1"
            GeometryCodeSurf_displ="1" GeometryCode_displ="3"
            H_c="0" K="0.1" Kf="22000000000" NumIPSurf_displ="9"
            NumIP_displ="27" Phi=".35" Psi=".1745" R="10" alpha_k="100"
            c0="0" constitutive_mod_type="2" field_name="displ"
            g="9.81" g1="0.0" g2="0.0" g3="0.0"
            initial_condition_1geostatic_2displ_vel_press="1"
            kappa0="-2000" lambda="29000000" local_tol_absolute="1e-8"
            local_tol_relative="1e-8" max_local_iterations="20"
            local_yield_tr_tol="1e-1"
            mu="7000000" n_en_displ="27" n_en_press="8" phi_f0="0.42"
            phi_s0="0.58" pore_pressure_field_name="press"
            rho_fR0="1000" rho_sR0="2700" type_of_analysis_0static_1consolidation_2dynamic="1">
            <total_lagrangian_solid_fluid_mix_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_element_block>
        </total_lagrangian_solid_fluid_mix>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+2" max_iterations="10000" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
