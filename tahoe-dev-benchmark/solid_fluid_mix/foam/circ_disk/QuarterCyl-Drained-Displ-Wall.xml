<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./QuarterCylHex27Coarse.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="compression of quarter symmetry foam disk with wall" 
    xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="2" num_steps="10" output_inc="1" time_step="1e-2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="1"/>
                <OrderedPair x="1" y="1"/>
                <OrderedPair x="1.1" y="0"/>
                <OrderedPair x="100" y="0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1011" type="fixed"/>
            <kinematic_BC dof="2" node_ID="1101" type="fixed"/>
            <kinematic_BC dof="1" node_ID="1222" type="fixed"/>
            <kinematic_BC dof="2" node_ID="1222" type="fixed"/>
            <kinematic_BC dof="3" node_ID="1110" type="fixed"/>
            <wall_penalty stiffness="1.5e-3">
                <bc_initial_position>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="5.15"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="-2.5"/>
                </bc_velocity>
                <velocity_schedule schedule="1"/>
                <node_ID_list>
                    <String value="1111"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="-1"/>
                </wall_normal>
            </wall_penalty>
        </field>
        <field field_name="press">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" type="fixed"/>
        </field>
        <history_node_ID_list>
            <String value="1111"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix 
            field_name="displ" pore_pressure_field_name="press" 
            n_en_displ="27" n_en_press="8" 
            GeometryCodeSurf_displ="1" GeometryCode_displ="3" 
            NumIPSurf_displ="9" NumIP_displ="27" 
            K="10" Kf=".1" 
            phi_f0="0.99" phi_s0="0.01"
            rho_fR0="1.2e-6" rho_sR0="1000e-6" 
            constitutive_mod_type="1" 
            mu="1923e-6" lambda="2885e-6" 
            Beta="-1" c0="0" H_c="0" Phi=".35" Psi=".1745" 
            R="10" alpha_k="100" kappa0="-2000" 
            local_tol_absolute="1e-8" local_tol_relative="1e-8" 
            local_yield_tr_tol="1e-1" max_local_iterations="20" 
            g="9.81" g1="0" g2="0" g3="0"
            initial_condition_1geostatic_2displ_vel_press="1"
            type_of_analysis_0static_1consolidation_2dynamic="0">
            <total_lagrangian_solid_fluid_mix_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_element_block>
            <gravity_body_force gravity_schedule="1"/>
        </total_lagrangian_solid_fluid_mix>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-14"
        divergence_tolerance="1.0e+2" max_iterations="50" rel_tolerance="1.0e-8">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
