<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="../geometry/cube.4.geom" geometry_format="TahoeII"
    output_format="Tahoe" title="one element 3D compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="0" num_steps="1" output_inc="1" time_step="1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
                <String value="d_z"/>
            </dof_labels>
            <kinematic_BC dof="3" node_ID="1"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="3"/>
        </field>
        <field field_name="press">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
        </field>
        <history_node_ID_list>
            <String value="1"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix GeometryCodeSurf_displ="1"
            GeometryCode_displ="3" Kf="100.0" NumIPSurf_displ="9"
            NumIP_displ="27" alpha="1" beta="0.3025"
            constitutive_mod_type="1" field_name="displ" gamma="0.6"
            lambda="1.5" mu="1" n_en_displ="20" n_en_press="8"
            phi_f0="0.5" phi_s0="0.5" pore_pressure_field_name="press"
            rho_fR0="1" rho_sR0="1">
            <total_lagrangian_solid_fluid_mix_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </total_lagrangian_solid_fluid_mix_element_block>
        </total_lagrangian_solid_fluid_mix>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
