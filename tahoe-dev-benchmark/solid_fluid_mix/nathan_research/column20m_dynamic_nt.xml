            <kinematic_BC dof="3" node_ID="4" type="fixed"/>
            <kinematic_BC dof="1" node_ID="6" type="fixed"/>
            <kinematic_BC dof="1" node_ID="7" type="fixed"/>
            <force_BC dof="3" node_ID="16" schedule="3" value="-253.287"/>
            <force_BC dof="3" node_ID="17" schedule="3" value="-1013.150"/>
            <force_BC dof="3" node_ID="15" schedule="3" value="-4052.590"/>
        </field>
        <field field_name="press" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="pore_press"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="16" type="fixed"/>
        </field>
    </nodes>
    <element_list>
        <total_lagrangian_solid_fluid_mix Beta="-1"
            GeometryCodeSurf_displ="1" GeometryCode_displ="3" H_c="0"
            K="0.01" Kf="2.2e10" NumIPSurf_displ="9" NumIP_displ="27"
            Phi=".35" Psi=".1745" R="10" alpha_k="1000" c0="0"
            constitutive_mod_type="2" field_name="displ" g="9.81" g1="0"
            g2="0" g3="-9.81"
            initial_condition_1geostatic_2displ_vel_press="1"
            kappa0="-2e5" lambda="8.4e6" local_tol_absolute="1e-8"
            local_tol_relative="1e-8" local_yield_tr_tol="1e-1"
            max_local_iterations="20" mu="5.6e6" n_en_displ="27"
            n_en_press="8" phi_f0="0.42" phi_s0="0.58"
            pore_pressure_field_name="press" rho_fR0="1000"
