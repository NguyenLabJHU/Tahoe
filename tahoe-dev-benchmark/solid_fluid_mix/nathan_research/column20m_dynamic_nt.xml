            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <cosine a="1" b="-1" c="50" d="0"/>
        </schedule_function>
        <schedule_function>
            <cosine_plus a="1" b="-1" c="50" d="0"
                description="f(t) = H(t,t1-t0)*(a + b cos(c t) + d sin(e t) + f t cos(g t) + p t sin(q t))"
                e="0" f="0" g="0" p="0" q="0" t0=".5" t1=".75"/>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ" integrator="nonlinear_HHT">
