<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="toohey" echo_input="true" geometry_file="./cubit01.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="ten 27-node elements 3D compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="5" num_steps="40" output_inc="1" time_step="0.025">
