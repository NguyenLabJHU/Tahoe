<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true" geometry_file="mesh.eye03.g"
    geometry_format="ExodusII" output_format="ExodusII"
    title="lens compress" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="5" num_steps="1800" output_inc="1" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="1.0"/>
                <OrderedPair x="8.9" y="1.0"/>
                <OrderedPair x="9.1" y="0"/>
                <OrderedPair x="180" y="0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <wall_penalty stiffness="1.5e4">
                <bc_initial_position>
                    <Vector_2 v_1="0.0" v_2="0.0"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_2 v_1="0" v_2="0"/>
                </bc_velocity>
                <velocity_constant/>
                <node_ID_list>
                    <String value="6"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_2 v_1="0.0" v_2="1"/>
                </wall_normal>
            </wall_penalty>
            <wall_penalty stiffness="1.5e4">
                <bc_initial_position>
                    <Vector_2 v_1="0.0" v_2="8.936"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_2 v_1="0.0" v_2="-0.1"/>
                </bc_velocity>
                <velocity_schedule schedule="1"/>
                <node_ID_list>
                    <String value="7"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_2 v_1="0.0" v_2="-1"/>
                </wall_normal>
            </wall_penalty>
        </field>
        <history_node_ID_list>
            <String value="6"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <updated_lagrangian_axi field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_axi_element_block>
                <block_ID_list>
                    <String value="2"/>
                    <String value="3"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Reese-Govindjee_split density="1"
                        eta_bulk="{100*MUi*TAU}"
			eta_shear="{MUi*TAU}"
                        kappa_EQ="{100*MUoo}"
			kappa_NEQ="{100*MUi}"
                        mu_EQ="MUoo"
			mu_NEQ="MUi"/>
                </large_strain_material_3D>
            </large_strain_axi_element_block>
        </updated_lagrangian_axi>
        <updated_lagrangian_axi field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <large_strain_axi_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Reese-Govindjee_split density="1" 
			eta_bulk="{1e-1*MUi*TAU}"
                        eta_shear="{1e-4*MUi*TAU}" 
			kappa_EQ="{1e-1*MUoo}"
                        kappa_NEQ="{1e-1*MUi}" 
			mu_EQ="{1e-4*MUoo}" 
			mu_NEQ="{1e-4*MUi}"/>
                </large_strain_material_3D>
            </large_strain_axi_element_block>
        </updated_lagrangian_axi>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+03" max_iterations="20" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
