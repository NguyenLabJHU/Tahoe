# DAKOTA INPUT FILE : search.in

strategy, \
	single_method \
	tabular_graphics_data \
	graphics

method, \
	optpp_q_newton, \
	convergence_tolerance = 1e-8 \
	output verbose \

variables, \
        continuous_design = 3 \
        cdv_descriptor 'TAU' 'MUoo' 'MUi' \
        cdv_lower_bounds  1000.0 0.05 0.05  \
        cdv_upper_bounds  3000.0 3.0  5.0 

interface, \
	system \
	analysis_driver = 'error_func.csh' \
	parameters_file = 'error_func.in' \
	results_file = 'error_func.out' \
	file_tag \
	file_save

responses, \
	num_objective_functions = 1 \
	numerical_gradients \
	method_source dakota \
	interval_type central \
	fd_step_size = 0.001 \
	no_hessians

