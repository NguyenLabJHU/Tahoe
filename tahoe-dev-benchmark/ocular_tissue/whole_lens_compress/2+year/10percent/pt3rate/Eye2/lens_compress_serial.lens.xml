<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="mesh.lens2.g" geometry_format="ExodusII"
    output_format="ExodusII" title="lens compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="2" num_steps="1800" output_inc="1" time_step=".2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="1.0"/>
                <OrderedPair x="3.0" y="1.0"/>
                <OrderedPair x="3.2" y="0"/>
                <OrderedPair x="180" y="0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <wall_penalty stiffness="1.5e4">
                <bc_initial_position>
                    <Vector_2 v_1="0.0" v_2="0.0"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_2 v_1="0" v_2="0"/>
                </bc_velocity>
                <velocity_constant/>
                <node_ID_list>
                    <String value="6"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_2 v_1="0.0" v_2="1"/>
                </wall_normal>
            </wall_penalty>
            <wall_penalty stiffness="1.5e4">
                <bc_initial_position>
                    <Vector_2 v_1="0.0" v_2="8.945"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_2 v_1="0.0" v_2="-0.3"/>
                </bc_velocity>
                <velocity_schedule schedule="1"/>
                <node_ID_list>
                    <String value="7"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_2 v_1="0.0" v_2="-1"/>
                </wall_normal>
            </wall_penalty>
        </field>
        <history_node_ID_list>
            <String value="6"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <updated_lagrangian_axi field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <large_strain_axi_element_block>
                <block_ID_list>
                    <String value="2"/>
                    <String value="3"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Reese-Govindjee_split density="1" eta_bulk="500.0"
                        eta_shear="5.0" kappa_EQ="136.0"
                        kappa_NEQ="50.0" mu_EQ="1.36" mu_NEQ="0.5"/>
                </large_strain_material_3D>
            </large_strain_axi_element_block>
        </updated_lagrangian_axi>
        <updated_lagrangian_axi field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <large_strain_axi_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Reese-Govindjee_split density="1" eta_bulk="0.50"
                        eta_shear="5.0e-4" kappa_EQ="0.136"
                        kappa_NEQ="0.05" mu_EQ="1.36e-4" mu_NEQ="5.0e-5"/>
                </large_strain_material_3D>
            </large_strain_axi_element_block>
        </updated_lagrangian_axi>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+03" max_iterations="20" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
