#!/usr/bin/perl -w

$outfile = "error_sum";

$sum = 0;

foreach $infile (@ARGV) {
	open(IN,$infile) || die "cannot open file $infile for reading : $!";
	$_ = <IN> ; s/^\s+//; s/\s+$//; ($error, $name) = split(/ +/,$_);
	close(IN);
	$sum += 1.0*$error;
}

open(OUT,">$outfile") || die "cannot create $outfile: $!";
print OUT "$sum $name\n";
close(OUT);
