<?xml version="1.0" encoding="UTF-8"?>
<tahoe geometry_file="./tension-strip.exo" geometry_format="ExodusII"
   output_format="Tahoe" 
   title="tension-strip" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="1" num_steps="181" output_inc="1" time_step="0.2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0.0"/>
                <OrderedPair x="12.2" y="0.62"/>
                <OrderedPair x="36.2" y="0.62"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="4"/>
            <kinematic_BC dof="3" node_ID="4"/>
            <kinematic_BC dof="1" node_ID="6" schedule="1" type="u" value="1"/>
            <kinematic_BC dof="2" node_ID="6"/>
            <kinematic_BC dof="3" node_ID="6"/>
        </field>
        <history_node_ID_list>
            <String value="6"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <updated_lagrangian_Q1P0 field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                principal_stress="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
<!--
                    <Isotropic_Cornea_Model density="1">
                        <E_and_nu Poisson_ratio=".25" Young_modulus="1"/>
                        <icosahedral points="40"/>
                        <fung_type_chain_power_repulsion
                            exponent_beta="54.11437*BETA}"
                            multiplier_C1="0.0273627*C1}"
                            repulsion_exponent="6"/>
                    </Isotropic_Cornea_Model>
-->
                    <Isotropic_Viscoelastic_Cornea_Model density="1">
                        <icosahedral points="40"/>
                        <eq_fung_rep 
                            exponent_beta="17.5832973385165"
                            multiplier_C1="0.0222142037528642" 
                            repulsion_exponent="6"/>
                        <neq_fung_rep 
                            exponent_beta="13.0114370609275"
                            multiplier_C1="0.07954707740532" 
                            repulsion_exponent="6"/>
                        <linear_viscosity
                            initial_bulk_relaxation_time="2086.9133002116" 
                            initial_shear_relaxation_time="1252.14798012696"/>
                    </Isotropic_Viscoelastic_Cornea_Model>
                </large_strain_material_3D>
            </large_strain_element_block>
        </updated_lagrangian_Q1P0>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+03" max_iterations="20" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
