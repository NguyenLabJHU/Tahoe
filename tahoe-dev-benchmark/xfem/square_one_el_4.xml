<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./geometry/square.q4.geom" geometry_format="TahoeII"
    output_format="Tahoe" title="one 4-node element 2D compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="3" num_steps="1" output_inc="1" time_step="1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="2" node_ID="3" schedule="1" type="u" value="0.1"/>
        </field>
        <field field_name="enhanced">
            <dof_labels>
                <String value="q1_x"/>
                <String value="q1_y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="4" type="fixed"/>
            <kinematic_BC dof="2" node_ID="4" type="fixed"/>
        </field>
    </nodes>
    <element_list>
        <xfem_SS_2D GeometryCodeSurf_displ="0" GeometryCode_displ="1"
            NumIPSurf_displ="2" NumIP_displ="4"
            constitutive_mod_type="1" enhan_field_name="enhanced"
            field_name="displ" lambda="29000000" mu="7000000"
            n_en_displ="4" n_en_enhan="4" ndof_per_nd_enhan="2">
            <xfem_SS_2D_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
            </xfem_SS_2D_element_block>
        </xfem_SS_2D>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+2" max_iterations="10" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
