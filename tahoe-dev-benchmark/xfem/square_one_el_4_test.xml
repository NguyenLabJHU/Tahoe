<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="regueiro" echo_input="true"
    geometry_file="./geometry/square.q4.geom" geometry_format="TahoeII"
    output_format="Tahoe" title="one 4-node element 2D compression test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="3" num_steps="1" output_inc="1" time_step="1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displ">
            <dof_labels>
                <String value="d_x"/>
                <String value="d_y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="2" node_ID="3" schedule="1" type="u" value="0.1"/>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displ">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <solid_element_element_output stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_J2_PlaneStrain density="1">
                        <E_and_nu Poisson_ratio=".403" Young_modulus="19.64e6"/>
                        <linear_function a="1" b="1e7"/>
                    </small_strain_StVenant_J2_PlaneStrain>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-20"
        divergence_tolerance="1.0e+2" max_iterations="10" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
