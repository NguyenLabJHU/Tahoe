            <kinematic_BC dof="3" node_ID="2"/>
            <kinematic_BC dof="2" node_ID="3"/>
            <kinematic_BC dof="1" node_ID="4"/>
            <kinematic_BC dof="2" node_ID="5"/>
            <kinematic_BC dof="1" node_ID="6"/>
            <kinematic_BC dof="2" node_ID="9"/>
            <kinematic_BC dof="1" node_ID="10"/>
            <kinematic_BC dof="3" node_ID="7" schedule="2" type="u" value="-0.01"/>
        </field>
        <history_node_ID_list>
            <String value="7"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant_DP_Loc density="1">
                        <E_and_nu Poisson_ratio=".3" Young_modulus="1.5e7"/>
                        <DP_Loc_SS_linear_hardening H="-1e6"
                            dilation=".35" fluidity_eta="0"
                            friction=".5" kappa="1.3e4"/>
                    </small_strain_StVenant_DP_Loc>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant density="1.0">
                        <E_and_nu Poisson_ratio="0.13" Young_modulus="1e10"/>
                    </small_strain_StVenant>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
