<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="skyu" geometry_file="../cse.geom"
    geometry_format="TahoeII" output_format="Tahoe"
    title="oriented EP cse" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="2" num_steps="100" output_inc="1" time_step=".01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.1" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.1" y="1"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
            <kinematic_BC dof="2" node_ID="2" schedule="1" type="u" value="-0.0045"/>
            <force_BC dof="1" node_ID="4" schedule="2" value="700"/>
            <force_BC dof="1" node_ID="5" schedule="2" value="100"/>
            <force_BC dof="1" node_ID="6" schedule="2" value="-720"/>
            <force_BC dof="1" node_ID="7" schedule="2" value="-80"/>
        </field>
        <history_node_ID_list>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_DP_2D density="1">
                        <E_and_nu Poisson_ratio=".3" Young_modulus="1.5e7"/>
                        <DP_SS_linear_hardening H_prime="-1e6"
                            alpha_bar="1.3e4" dilation=".35" friction=".5"/>
                    </small_strain_StVenant_DP_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <nodal_rigid_anisotropic_CSE_MR field_name="displacement">
            <line/>
            <surface_element_nodal_output displacement_jump="1"
                displacements="1" material_output="1" traction="1"/>
            <anisotropic_CSE_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <cohesive_relation_2D>
                    <nodal-rigid-plastic_MR_RP2D Gf_I="2" Gf_II="2"
                        Tol_1="1e-8" Tol_2="1e-8" alpha_c="1e3"
                        alpha_chi="1e3" alpha_phi="9e2" alpha_psi="9e2"
                        c_p="9053" c_r="0" chi_p="15681" chi_r="0"
                        phi_p="0.5236" phi_r="0" psi_p="0.087"/>
                </cohesive_relation_2D>
            </anisotropic_CSE_element_block>
        </nodal_rigid_anisotropic_CSE_MR>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+10" max_iterations="100" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
