<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="skyu" geometry_file="../cse_complex.geom"
    geometry_format="TahoeII" output_format="ExodusII"
    title="oriented EP cse" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="50" num_steps="1000" output_inc="1" time_step="0.001">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.1" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="0.005" y="1"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="1"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="10" type="fixed"/>
            <kinematic_BC dof="1" node_ID="7" type="fixed"/>
            <kinematic_BC dof="1" node_ID="6" type="fixed"/>
            <kinematic_BC dof="2" node_ID="8" schedule="3" type="u" value="-0.01"/>
            <kinematic_BC dof="1" node_ID="3" schedule="3" type="u" value="0.005"/>
            <force_BC dof="1" node_ID="4" schedule="4" value="400"/>
            <force_BC dof="1" node_ID="5" schedule="4" value="-700"/>
            <force_BC dof="2" node_ID="9" schedule="4" value="-450"/>
            <force_BC dof="2" node_ID="11" schedule="4" value="600"/>
        </field>
        <history_node_ID_list>
            <String value="8"/>
            <String value="3"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_2D
                        constraint_2D="plane_strain" density="1">
                        <E_and_nu Poisson_ratio=".3" Young_modulus="1.5e7"/>
                    </small_strain_StVenant_2D>
                </small_strain_material_2D>
            </small_strain_element_block>
        </small_strain>
        <anisotropic_CSE field_name="displacement">
            <line/>
            <surface_element_nodal_output displacements="1"
                material_output="1" traction="1"/>
            <anisotropic_CSE_element_block>
                <block_ID_list>
                    <String value="2"/>
                </block_ID_list>
                <cohesive_relation_2D>
                    <elastoplastic_MR_2D E_n="1e9" E_t="1e9" Gf_I="5e4"
                        Gf_II="5e4" Tol_1="1e-8" Tol_2="1e-8"
                        alpha_c="2e3" alpha_chi="2e3" alpha_phi="1e3"
                        alpha_psi="1e3" c_p="9053" c_r="0.0"
                        chi_p="15680" chi_r="0" phi_p="0.5236"
                        phi_r="0.0" psi_p="0.087"/>
                </cohesive_relation_2D>
            </anisotropic_CSE_element_block>
        </anisotropic_CSE>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+10" max_iterations="50" rel_tolerance="1.0e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
