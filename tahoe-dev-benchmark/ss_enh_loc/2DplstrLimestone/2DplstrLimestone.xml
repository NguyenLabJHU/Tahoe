<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="raregue" geometry_file="./fLimestoneBau2D.geom"
    title="2D plane strain compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="40" output_inc="1" time_step="2.5e-2">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="2" schedule="2" type="u" value="-0.002"/>
            <kinematic_BC dof="1" node_ID="4"/>
        </field>
    </nodes>
    <element_list>
        <small_strain_enh_loc choose_element="1" choose_normal="2"
            cohesion_softening_coefficient="1e3"
            dilation_softening_coefficient="0" field_name="displacement"
            friction_softening_coefficient="0" model_type="1"
            peak_cohesion="0" peak_dilation_angle_rad="0.087"
            peak_friction_angle_rad=".5236" residual_cohesion="0" residual_friction_angle_rad="0">
            <quadrilateral/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_enh_loc_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <Fossum_small_strain_2D
                        cap_growth_parameter__D1__1_by_stress="1.47e-3"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless=".08"
                        cap_ratio__R__dimensionless="28" density="1.0"
                        fluidity_parameter__eta__dimensionless="0"
                        initial_cap_position__kappa_0__stress="-8.05"
                        shear_surface_growth_factor__c_alpha__stress="1e5"
                        shear_surface_offset__N__stress="6"
                        shear_surface_parameter__A__stress="689.2"
                        shear_surface_parameter__B__1_by_stress="3.94e-4"
                        shear_surface_parameter__C__stress="675.2"
                        shear_surface_parameter__theta__radians="0" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1">
                        <E_and_nu Poisson_ratio=".25" Young_modulus="22547"/>
                    </Fossum_small_strain_2D>
                </small_strain_material_2D>
            </small_strain_enh_loc_element_block>
            <start_surface_coord>
                <Vector_2 v_1="0.04" v_2="0.079"/>
            </start_surface_coord>
        </small_strain_enh_loc>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-12"
        divergence_tolerance="1.0e+03" max_iterations="50" rel_tolerance="1.0e-12">
        <full_matrix/>
    </nonlinear_solver>
</tahoe>
