<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="bao" echo_input="true" geometry_file="cylinder.g"
    geometry_format="ExodusII" output_format="ExodusII" title="cylinder" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="10" output_inc="1" time_step="0.1">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1" y="1"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_x"/>
                <String value="D_y"/>
                <String value="D_z"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="40"/>
            <kinematic_BC dof="1" node_ID="50"/>
            <kinematic_BC dof="3" node_ID="30"/>
            <wall_penalty stiffness="1e4">
                <bc_initial_position>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="5.0"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="0.0"/>
                </bc_velocity>
                <velocity_constant/>
                <node_ID_list>
                    <String value="30"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="-1.0"/>
                </wall_normal>
            </wall_penalty>
            <wall_penalty stiffness="1e4">
                <bc_initial_position>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="-5.0"/>
                </bc_initial_position>
                <bc_velocity>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="2.0"/>
                </bc_velocity>
                <velocity_schedule schedule="1"/>
                <node_ID_list>
                    <String value="20"/>
                </node_ID_list>
                <wall_normal>
                    <Vector_3 v_1="0.0" v_2="0.0" v_3="1.0"/>
                </wall_normal>
            </wall_penalty>
        </field>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <solid_element_nodal_output displacements="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <Fossum_small_strain
                        cap_growth_parameter__D1__1_by_stress="9.28e-10"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless="0.154"
                        cap_ratio__R__dimensionless="10" density="1.0"
                        fluidity_parameter__eta__dimensionless="0"
                        initial_cap_position__kappa_0__stress="2.93e6"
                        shear_surface_growth_factor__c_alpha__stress="0"
                        shear_surface_offset__N__stress="0"
                        shear_surface_parameter__A__stress="1.26e9"
                        shear_surface_parameter__B__1_by_stress="1.93e-10"
                        shear_surface_parameter__C__stress="1.25e9"
                        shear_surface_parameter__theta__radians="1.0e-10" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1.0">
                        <E_and_nu Poisson_ratio="0.13" Young_modulus="3.18e10"/>
                    </Fossum_small_strain>
                </small_strain_material_3D>
            </small_strain_element_block>
        </small_strain>
    </element_list>
    <nonlinear_solver abs_tolerance="1e-10" divergence_tolerance="1e2"
        max_iterations="10" rel_tolerance="1e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
