<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="raregue" echo_input="true"
    geometry_file="../geometry/cube.geom" geometry_format="TahoeII"
    output_format="Tahoe" title="uniaxial compression" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time max_step_cuts="3" num_steps="100" output_inc="1" time_step="1e-2">
        <schedule_function>
            <linear_exponential a="0" b="0" c="-1" d="1000"/>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_x"/>
                <String value="D_y"/>
                <String value="D_z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1"/>
            <kinematic_BC dof="2" node_ID="2"/>
            <kinematic_BC dof="3" node_ID="3"/>
            <kinematic_BC dof="3" node_ID="4" schedule="1" type="u" value="1.0"/>
        </field>
    </nodes>
    <element_list>
        <small_strain_enh_loc choose_element="1" choose_normal="2"
            cohesion_softening_coefficient="2e7"
            dilation_softening_coefficient="1e2"
            field_name="displacement"
            friction_softening_coefficient="1e2" model_type="0"
            peak_cohesion="0" peak_dilation_angle_rad=".087"
            peak_friction_angle_rad=".5236" residual_cohesion="0" residual_friction_angle_rad="0">
            <hexahedron/>
            <solid_element_element_output material_output="1" stress="1"/>
            <small_strain_enh_loc_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <Fossum_small_strain
                        cap_growth_parameter__D1__1_by_stress="9.28e-10"
                        cap_growth_parameter__D2__1_by_stress_squared="0"
                        cap_growth_parameter__W__dimensionless="0.154"
                        cap_ratio__R__dimensionless="10" density="1.0"
                        fluidity_parameter__eta__dimensionless="0"
                        initial_cap_position__kappa_0__stress="2.93e6"
                        shear_surface_growth_factor__c_alpha__stress="0"
                        shear_surface_offset__N__stress="0"
                        shear_surface_parameter__A__stress="1.26e9"
                        shear_surface_parameter__B__1_by_stress="1.93e-10"
                        shear_surface_parameter__C__stress="1.25e9"
                        shear_surface_parameter__theta__radians="1.0e-10" triaxial_compression_to_extension_strength_ratio__psi__dimensionless="1.0">
                        <E_and_nu Poisson_ratio="0.13" Young_modulus="3.18e10"/>
                    </Fossum_small_strain>
                </small_strain_material_3D>
            </small_strain_enh_loc_element_block>
        </small_strain_enh_loc>
    </element_list>
    <nonlinear_solver abs_tolerance="1e-10" divergence_tolerance="1e4"
        max_iterations="10" rel_tolerance="1e-10">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
