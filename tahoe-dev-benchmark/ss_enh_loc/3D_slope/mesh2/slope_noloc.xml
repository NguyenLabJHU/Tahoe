        <history_node_ID_list>
            <String value="1"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <small_strain field_name="displacement">
            <hexahedron/>
            <body_force schedule="1">
                <Double value="0"/>
                <Double value="0"/>
                <Double value="-2e1"/>
            </body_force>
            <solid_element_nodal_output displacements="1"
                material_output="1" stress="1"/>
            <small_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_3D>
                    <small_strain_StVenant_DP density="1.0">
