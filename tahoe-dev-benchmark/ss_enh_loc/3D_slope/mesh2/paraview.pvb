set saveState 0
for {set i  0} {$i < [expr $argc - 1]} {incr i} {
  if {[lindex $argv $i] == "-XML"} {
    set saveState 1
    set stateName [lindex $argv [expr $i + 1]]
  }
}
if { $saveState } {
   $Ren1 UpdateVTKObjects
   $proxyManager SaveState $stateName
} else {

$Ren1 UpdateVTKObjects
set inBatch 0
for {set i  1} {$i < [expr $argc]} {incr i} {
  if {[lindex $argv $i] == "-BV"} {
    set inBatch 1
  }
}
if { $inBatch } {
  set xsize [[$Ren1 GetProperty Size] GetElement 0]
  set ysize [[$Ren1 GetProperty Size] GetElement 1]
  $Ren1 TileWindows [expr $xsize+30] [expr $ysize+30] 2
}
$Ren1 WriteImage {/walnut/scratch/regueiro/development_benchmark_XML/ss_enh_loc/3D_slope/mesh2/paraview.jpg} vtkJPEGWriter
}

