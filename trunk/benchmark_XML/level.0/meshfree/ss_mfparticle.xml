<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="cjkimme" geometry_file="../geometry/particles.geom"
    title="2D small strain scnimf" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="10.0" y="2.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="2" node_ID="9" type="fixed"/>
            <augmented_Lagrangian_KBC_meshfree element_group="1" regularization="10.0">
                <kinematic_BC dof="1" node_ID="4" schedule="1" type="u" value="-0.00125"/>
                <kinematic_BC dof="1" node_ID="2" schedule="1" type="u" value="0.00125"/>
            </augmented_Lagrangian_KBC_meshfree>
        </field>
    </nodes>
    <element_list>
        <ss_mfparticle field_name="displacement" num_ip="4">
            <meshfree_support_2D>
                <RKPM>
                    <cubic_spline_window support_scaling="1.5"/>
                </RKPM>
            </meshfree_support_2D>
            <mf_particle_ID_list>
                <String value="9"/>
            </mf_particle_ID_list>
            <ss_connectivity_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <small_strain_material_2D>
                    <small_strain_StVenant_2D density="1.">
                        <E_and_nu Poisson_ratio="0.25" Young_modulus="100."/>
                    </small_strain_StVenant_2D>
                </small_strain_material_2D>
            </ss_connectivity_element_block>
        </ss_mfparticle>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-10"
        divergence_tolerance="1.0e+01" max_iterations="10" rel_tolerance="1.0e-12">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
