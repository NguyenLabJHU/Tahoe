<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="paklein" geometry_file="../geometry/beams.1.geom"
    title="K-field test" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="2" output_inc="1" time_step="0.5">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0.0" y="0.0"/>
                <OrderedPair x="1.0" y="1.0"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <K_field>
                <isotropic>
                    <E_and_nu Poisson_ratio="" Young_modulus=""/>
                </isotropic>
                <tip_tracking max_growth_distance="" max_growth_steps=""
                    near_tip_group="" near_tip_output_code="" tip_output_column="">
                    <location_of_maximum/>
                </tip_tracking>
            </K_field>
        </field>
    </nodes>
    <linear_solver/>
</tahoe>
